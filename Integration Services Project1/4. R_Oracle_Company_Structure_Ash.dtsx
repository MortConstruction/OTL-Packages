                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT   pt.project_id,
         pt.top_task_id,
         pt.task_id,
         top.task_number Top_Task_Number,
         top.task_name TOP_task_name,
         l2.task_id Parent_Task_id,
         l2.task_number Parent_task_number,
         l2.task_name Parent_task_name,
         pt.task_number,
         pt.task_name,
         pt.wbs_level,
         pt.creation_date,
         pt.start_date,
         pt.completion_date,
         pt.billable_flag,
		 substr(pt.attribute1,
                       instr(pt.attribute1,
                             '.') + 1,
                       3) BillCode,
		pt.attribute8 as MortPackage,
		pt.attribute7 as StandCostCode,
		pt.Attribute2 as WorkersCompReference,
		pt.Attribute3 as WorkersCompCode
FROM     pa.pa_tasks PT JOIN pa.pa_tasks TOP ON pt.top_task_id = top.task_id
         JOIN pa.pa_tasks L2 ON NVL(pt.parent_task_id, pt.task_id) = l2.task_id
	 and pt.task_id in (select distinct task_id from (
							select distinct project_id, task_id from pa.pa_expenditure_items_all
							union 
							select  project_id,task_id 
							from pa.pa_tasks pt
							where pt.CHARGEABLE_FLAG = 'Y'
							union
							select distinct  project_id,task_id from PA.pa_budget_lines pbl
							inner JOIN PA.pa_resource_assignments pra
								    ON pbl.resource_assignment_id = pra.resource_assignment_id ) 
								)
ORDER BY pt.project_id,
         top.task_id,
         l2.task_id,
         pt.task_id</property>
