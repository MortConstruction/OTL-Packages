                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">/*Org_ID To Company Segment*/
 select 'Segment1' as GLSegment, psvls.segment_VAlue_lookup_set_name,psvls.last_update_date
 ,psvl.Segment_Value_lookup as ORG_IDFromSegment, segment_Value as CompanyToSegment
 from  apps.pa_segment_value_lookups psvl
      ,apps.pa_segment_value_lookup_sets psvls
 WHERE psvl.segment_value_lookup_set_id = psvls.segment_value_lookup_set_id
  AND psvls.segment_value_lookup_set_name = 'Company From Project Org ID'
Union All
/*Carrying Out Org_ID To Group Segment*/
 select 'Segment2' as GLSegment,psvls.segment_VAlue_lookup_set_name,psvls.last_update_date
 ,psvl.Segment_Value_lookup as ORG_IDFromSegment, segment_Value as GroupToSegment
 from  apps.pa_segment_value_lookups psvl
      ,apps.pa_segment_value_lookup_sets psvls
 WHERE psvl.segment_value_lookup_set_id = psvls.segment_value_lookup_set_id
  AND psvls.segment_value_lookup_set_name = 'Group From Carry Out Org ID'

Union All
/*Carrying Out Org_ID To Function Segment*/
 select 'Segment3' as GLSegment,psvls.segment_VAlue_lookup_set_name,psvls.last_update_date
 ,psvl.Segment_Value_lookup as ORG_IDFromSegment, segment_Value as FunctionToSegment
 from  apps.pa_segment_value_lookups psvl
      ,apps.pa_segment_value_lookup_sets psvls
 WHERE psvl.segment_value_lookup_set_id = psvls.segment_value_lookup_set_id
  AND psvls.segment_value_lookup_set_name = 'Function From Carry Out Org ID'
order by 1,2,3,4</property>
