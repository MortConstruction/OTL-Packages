    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{A85CC9F0-2AE7-4D3F-A0FC-7901B1D0A907}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT   pt.project_id,&#xA;         pt.top_task_id,&#xA;         pt.task_id,&#xA;         top.task_number Top_Task_Number,&#xA;         top.task_name TOP_task_name,&#xA;         l2.task_id Parent_Task_id,&#xA;         l2.task_number Parent_task_number,&#xA;         l2.task_name Parent_task_name,&#xA;&#x9;       l2.task_name Parent_task_name2,&#xA;         pt.task_number,&#xA;         pt.task_name,&#xA;         pt.wbs_level,&#xA;         pt.creation_date,&#xA;         pt.start_date,&#xA;         pt.completion_date,&#xA;         pt.billable_flag,&#xA;&#x9;&#x9; substr(pt.attribute1,&#xA;                       instr(pt.attribute1,&#xA;                             '.') + 1,&#xA;                       3) BillCode,&#xA;&#x9;&#x9;pt.attribute8 as MortPackage,&#xA;&#x9;&#x9;pt.attribute7 as StandCostCode,&#xA;&#x9;&#x9;pt.Attribute2 as WorkersCompReference,&#xA;&#x9;&#x9;pt.Attribute3 as WorkersCompCode&#xA;FROM     pa.pa_tasks PT JOIN pa.pa_tasks TOP ON pt.top_task_id = top.task_id&#xA;         JOIN pa.pa_tasks L2 ON NVL(pt.parent_task_id, pt.task_id) = l2.task_id&#xA;&#x9; and pt.task_id in&#xA;(&#xA;select distinct pt.task_id from pa.pa_tasks pt&#xA;INNER JOIN pa.pa_expenditure_items_all peia&#xA;on peia.project_id = pt.project_id &#xA;and peia.task_id = pt.Task_id&#xA;where to_char(pt.last_update_date,'yyyy') = &quot; + @[User::vsActualsStartDate] &#xA;+ &quot; UNION select pt.task_id from pa.pa_tasks pt&#xA;where pt.CHARGEABLE_FLAG = 'Y' and to_char(pt.last_update_date,'yyyy') =  &quot; + @[User::vsActualsStartDate] &#xA;+ &quot; UNION&#xA;select distinct  s.task_id &#xA;from &#xA;((select &#xA;pt.project_id,pt.task_id &#xA;from pa.pa_tasks pt&#xA;where  to_char(pt.last_update_date,'yyyy') =  &quot; + @[User::vsActualsStartDate] &#xA;+ &quot;  ) s&#xA;INNER JOIN  PA.pa_resource_assignments pra&#xA;on pra.project_id = s.project_id&#xA;and pra.task_id = s.Task_id) &#xA;)&#xA;&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="vsSQL_GetTasks">
      <DTS:VariableValue
        DTS:DataType="8">SELECT   pt.project_id,
         pt.top_task_id,
         pt.task_id,
         top.task_number Top_Task_Number,
         top.task_name TOP_task_name,
         l2.task_id Parent_Task_id,
         l2.task_number Parent_task_number,
         l2.task_name Parent_task_name,
	       l2.task_name Parent_task_name2,
         pt.task_number,
         pt.task_name,
         pt.wbs_level,
         pt.creation_date,
         pt.start_date,
         pt.completion_date,
         pt.billable_flag,
		 substr(pt.attribute1,
                       instr(pt.attribute1,
                             '.') + 1,
                       3) BillCode,
		pt.attribute8 as MortPackage,
		pt.attribute7 as StandCostCode,
		pt.Attribute2 as WorkersCompReference,
		pt.Attribute3 as WorkersCompCode
FROM     pa.pa_tasks PT JOIN pa.pa_tasks TOP ON pt.top_task_id = top.task_id
         JOIN pa.pa_tasks L2 ON NVL(pt.parent_task_id, pt.task_id) = l2.task_id
	 and pt.task_id in
(
select distinct pt.task_id from pa.pa_tasks pt
INNER JOIN pa.pa_expenditure_items_all peia
on peia.project_id = pt.project_id 
and peia.task_id = pt.Task_id
where to_char(pt.last_update_date,'yyyy') = 2019 UNION select pt.task_id from pa.pa_tasks pt
where pt.CHARGEABLE_FLAG = 'Y' and to_char(pt.last_update_date,'yyyy') =  2019 UNION
select distinct  s.task_id 
from 
((select 
pt.project_id,pt.task_id 
from pa.pa_tasks pt
where  to_char(pt.last_update_date,'yyyy') =  2019  ) s
INNER JOIN  PA.pa_resource_assignments pra
on pra.project_id = s.project_id
and pra.task_id = s.Task_id) 
)
</DTS:VariableValue>
    </DTS:Variable>
  </DTS:Variables>
