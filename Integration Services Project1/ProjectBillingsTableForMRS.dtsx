                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select
inv.project_id
,inv.ra_invoice_number
,inv.draft_invoice_num
,inv.draft_invoice_num_credited
,item.invoice_line_type
,INV.invoice_date
,trunc(inv.gl_date, 'MONTH') as gl_month
/*, (trunc(pcdl.gl_date, ''''MONTH'''')) gl_month */
,inv.retn_code_combination_id
,inv.receivable_code_combination_id
,(item.amount) as TotalInvoiceAmount
,(case when item.invoice_line_type = 'STANDARD' THEN (item.amount) ELSE 0 END) as StandardAmount
,(case when item.invoice_line_type = 'RETENTION' THEN (item.amount) ELSE 0 END) as RetentionAmount
,(case when item.invoice_line_type = 'STANDARD' THEN 0 
      when item.invoice_line_type = 'RETENTION' THEN 0 
      else  (item.amount) end) as OtherAmount
from  apps.pa_draft_invoices_all inv
INNER JOIN apps.pa_draft_invoice_items item
on item.project_id = inv.project_id
and item.draft_invoice_num = inv.draft_invoice_num
where 1=1
/*and inv.project_id = 12630*/
and inv.released_date is not null</property>
