      <DTS:Variables />
      <DTS:ObjectData>
        <pipeline
          BLOBTempStoragePath=""
          bufferTempStoragePath=""
          engineThreads="5"
          version="1">
          <components>
            <component
              refId="Package\Indirect Transaction to Staging Table\New Oracle PA Billing Transactions for MRS"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation;Microsoft SqlServer v9; (C) 2005 Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="New Oracle PA Billing Transactions for MRS"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT pa.org_id,
       haou.NAME Organization_name,
       pa.carrying_out_organization_id,
       haou2.NAME Project_Organization_name,
       revenue.project_id,
       pa.segment1 project_number,
       pa.NAME project_name,
       revenue.segment1,
       revenue.segment2,
       revenue.segment3,
       revenue.segment4,
       revenue.segment5,
       TRUNC(revenue.gl_date, 'MM') gl_date,
       TO_DATE(TRUNC(revenue.pa_date)) pa_date,
       (revenue.Revenue_Amount) Revenue_Amount
FROM   (SELECT   pdra.project_id,
                 pdra.gl_date,
                 pdra.pa_date,
                 0 AS Invoice_amount,
                 SUM(NVL(pdri.amount, 0)) AS Revenue_amount,
                   SUM(NVL(pdri.amount, 0))
                 * -1 amount,
                 'Draft2' AS SOURCE,
                 gl.segment1,
                 gl.segment2,
                 gl.segment3,
                 gl.segment4,
                 gl.segment5
        FROM     pa.pa_draft_revenues_all pdra JOIN pa.pa_draft_revenue_items pdri ON pdra.project_id = pdri.project_id
                 AND                                                                 pdra.draft_revenue_num = pdri.draft_revenue_num
                 AND                                                                 pdra.draft_revenue_num = pdri.draft_revenue_num
                 JOIN pa.pa_cust_event_rdl_all pcera ON pdra.project_id = pcera.project_id
                 AND                                   pdra.draft_revenue_num = pcera.draft_revenue_num
                 AND                                   pdri.line_num = pcera.draft_revenue_item_line_num
                 JOIN gl.gl_code_combinations gl ON pcera.code_combination_id = gl.code_combination_id
        GROUP BY pdra.project_id,
                 pdra.gl_date,
                 pdra.pa_date,
                 gl.segment1,
                 gl.segment2,
                 gl.segment3,
                 gl.segment4,
                 gl.segment5) revenue
       LEFT JOIN
       pa.pa_projects_all pa ON revenue.project_id = pa.project_id
       JOIN hr.hr_all_organization_units haou ON pa.org_id = haou.organization_id
       JOIN hr.hr_all_organization_units haou2 ON pa.carrying_out_organization_id = haou2.organization_id
WHERE  1 = 1
UNION ALL
SELECT   org_id,
         Organization_name,
         carrying_out_organization_id,
         Project_Organization_name,
         project_id,
         main2.segment1 Project_Number,
         NAME,
         build_gl.segment1,
         build_gl.segment2,
         build_gl.segment3,
         '310000' segment4,
         '000' segment5,
         TO_DATE('31-DEC-2005', 'DD-MON-YYYY') GL_date,
         TO_DATE('31-DEC-2005', 'DD-MON-YYYY') pa_date,
         SUM(Total_Amount) Total_Amount
FROM     (SELECT   pa.org_id,
                   pa.carrying_out_organization_id,
                   HAOU.NAME Organization_Name,
                   pa.project_type,
                   main.project_id,
                   HAOU2.NAME Project_Organization_Name,
                   pa.segment1,
                   pa.NAME,
                   'Revenue' AS expenditure_type,
                   'Revenue' AS transaction_source,
                   TRUNC(Trx_date_Adj, 'MM') Trx_date,
                   main.gl_date,
                   TRUNC(main.gl_date, 'MM') expenditure_item_date,
                   SUM(Total_Invoice_Amount) Total_Amount,
                   SUM(Regular_Invoice_Amount) Regular_Invoice_Amount,
                   SUM(Retention_Amount) Retention_Amount,
                   main.gl_period_name,
                   main.RECEIVABLE_CODE_COMBINATION_ID,
                   RETN_CODE_COMBINATION_ID,
                   Code_Combination_ID_adj,
                   gcc.segment1 segment1_COMPANY,
                   gcc.segment2 segment2_GROUP,
                   gcc.segment3 segment3_FUNCTION,
                   gcc.segment4 segment4_ACCOUNT,
                   gcc.segment5 segment4_Open
          FROM     (SELECT   inv.project_id,
                             inv.draft_invoice_num,
                             inv.ra_invoice_number,
                             inv.invoice_date,
                             inv.approved_date,
                             inv.released_date,
                             inv.creation_date,
                             inv.gl_date GL_Date,
                             TO_DATE('31-DEC-2005', 'DD-MON-YYYY') Trx_date_Adj,
                             TO_DATE('31-DEC-2005', 'DD-MON-YYYY') GL_Month_date,
                             DECODE(inv.gl_date,
                                    NULL, NULL,
                                    gl_per.period_name) GL_Period_Name,
                             pa_per.period_name PA_Period,
                             SUM(  item.amount
                                 - DECODE(item.invoice_line_type,
                                          'RETENTION', item.amount,
                                          0)) Total_Invoice_Amount,
                             -SUM(DECODE(item.invoice_line_type,
                                         'RETENTION', item.amount,
                                         0)) Retention_Amount,
                             SUM(item.amount) Regular_Invoice_Amount,
                             inv.draft_invoice_num_credited,
                             DECODE(item.invoice_line_type,
                                    'RETENTION', RETN_CODE_COMBINATION_ID,
                                    RECEIVABLE_CODE_COMBINATION_ID) Code_Combination_ID_adj,
                             inv.RECEIVABLE_CODE_COMBINATION_ID,
                             inv.RETN_CODE_COMBINATION_ID,
                             item.invoice_line_type
                    FROM     pa.pa_implementations_all imp,
                             pa.pa_periods_all pa_per,
                             gl.gl_period_statuses gl_per,
                             pa.pa_draft_invoice_items item,
                             pa.pa_draft_invoices_all inv
                    WHERE    inv.released_date IS NOT NULL
                    AND      item.project_id = inv.project_id
                    AND      item.draft_invoice_num = inv.draft_invoice_num
                    AND      gl_per.set_of_books_id = imp.set_of_books_id
                    AND      gl_per.application_id = apps.PA_Period_Process_Pkg.Application_ID
                    AND      gl_per.adjustment_period_flag = 'N'
                    AND      TO_DATE('31-DEC-2005', 'DD-MON-YYYY') BETWEEN gl_per.start_date AND gl_per.end_date
                    AND      inv.pa_date BETWEEN pa_per.start_date AND pa_per.end_date
                    AND      imp.org_id = inv.org_id
                    AND      imp.org_id = pa_per.org_id
                    AND      (   (    item.project_id IN(652, 968, 952, 909, 928, 794, 892, 3155, 1409)
                                  AND inv.draft_invoice_num = 1
                                  AND item.line_num = 1)
                              OR (    item.project_id IN(889, 877)
                                  AND inv.draft_invoice_num = 6
                                  AND item.line_num = 1)
                              OR (    item.project_id IN(933)
                                  AND inv.draft_invoice_num = 4
                                  AND item.line_num = 1)
                              OR (    item.project_id IN(755)
                                  AND inv.draft_invoice_num = 2
                                  AND item.line_num = 1))
                    GROUP BY inv.project_id,
                             inv.draft_invoice_num,
                             inv.ra_invoice_number,
                             inv.invoice_date,
                             inv.approved_date,
                             inv.released_date,
                             inv.creation_date,
                             inv.gl_date,
                             TRUNC(inv.gl_date, 'MM'),
                             CASE
                                WHEN inv.pa_period_name = 'DEC-05'
                             AND    inv.gl_period_name &lt;&gt; 'DEC-05'
                                   THEN TO_DATE('31-DEC-2005', 'DD-MON-YYYY')
                                ELSE TRUNC(inv.gl_date, 'MM')
                             END,
                             gl_per.period_name,
                             pa_per.period_name,
                             inv.draft_invoice_num_credited,
                             RECEIVABLE_CODE_COMBINATION_ID,
                             RETN_CODE_COMBINATION_ID,
                             item.invoice_line_type
                    UNION ALL
                    SELECT   inv.project_id,
                             inv.draft_invoice_num,
                             inv.ra_invoice_number,
                             inv.invoice_date,
                             inv.approved_date,
                             inv.released_date,
                             inv.creation_date,
                             inv.gl_date GL_Date,
                             CASE
                                WHEN inv.pa_period_name = 'DEC-05'
                             AND    inv.gl_period_name &lt;&gt; 'DEC-05'
                                   THEN TO_DATE('31-DEC-2005', 'DD-MON-YYYY')
                                ELSE TRUNC(inv.gl_date, 'MM')
                             END Trx_date_Adj,
                             TRUNC(inv.gl_date, 'MM') GL_Month_date,
                             DECODE(inv.gl_date,
                                    NULL, NULL,
                                    gl_per.period_name) GL_Period_Name,
                             pa_per.period_name PA_Period,
                             SUM(  item.amount
                                 - DECODE(item.invoice_line_type,
                                          'RETENTION', item.amount,
                                          0)) Total_Invoice_Amount,
                             -SUM(DECODE(item.invoice_line_type,
                                         'RETENTION', item.amount,
                                         0)) Retention_Amount,
                             SUM(item.amount) Regular_Invoice_Amount,
                             inv.draft_invoice_num_credited,
                             DECODE(item.invoice_line_type,
                                    'RETENTION', RETN_CODE_COMBINATION_ID,
                                    RECEIVABLE_CODE_COMBINATION_ID) Code_Combination_ID_adj,
                             inv.RECEIVABLE_CODE_COMBINATION_ID,
                             inv.RETN_CODE_COMBINATION_ID,
                             item.invoice_line_type
                    FROM     pa.pa_implementations_all imp,
                             pa.pa_periods_all pa_per,
                             gl.gl_period_statuses gl_per,
                             pa.pa_draft_invoice_items item,
                             pa.pa_draft_invoices_all inv
                    WHERE    inv.released_date IS NOT NULL
                    AND      item.project_id = inv.project_id
                    AND      item.draft_invoice_num = inv.draft_invoice_num
                    AND      gl_per.set_of_books_id = imp.set_of_books_id
                    AND      gl_per.application_id = apps.PA_Period_Process_Pkg.Application_ID
                    AND      gl_per.adjustment_period_flag = 'N'
                    AND      NVL(CASE
                                    WHEN inv.pa_period_name = 'DEC-05'
                                 AND    inv.gl_period_name &lt;&gt; 'DEC-05'
                                       THEN TO_DATE('31-DEC-2005', 'DD-MON-YYYY')
                                    ELSE TRUNC(inv.gl_date, 'MM')
                                 END, TRUNC(SYSDATE)) BETWEEN gl_per.start_date AND gl_per.end_date
                    AND      inv.pa_date BETWEEN pa_per.start_date AND pa_per.end_date
                    AND      inv.pa_period_name = 'DEC-05'
                    AND      imp.org_id = inv.org_id
                    AND      imp.org_id = pa_per.org_id
                    GROUP BY inv.project_id,
                             inv.draft_invoice_num,
                             inv.ra_invoice_number,
                             inv.invoice_date,
                             inv.approved_date,
                             inv.released_date,
                             inv.creation_date,
                             inv.gl_date,
                             TRUNC(inv.gl_date, 'MM'),
                             CASE
                                WHEN inv.pa_period_name = 'DEC-05'
                             AND    inv.gl_period_name &lt;&gt; 'DEC-05'
                                   THEN TO_DATE('31-DEC-2005', 'DD-MON-YYYY')
                                ELSE TRUNC(inv.gl_date, 'MM')
                             END,
                             gl_per.period_name,
                             pa_per.period_name,
                             inv.draft_invoice_num_credited,
                             RECEIVABLE_CODE_COMBINATION_ID,
                             RETN_CODE_COMBINATION_ID,
                             item.invoice_line_type) MAIN
                   JOIN
                   pa.pa_projects_all pa ON main.project_id = pa.project_id
                   JOIN HR.HR_ALL_ORGANIZATION_UNITS HAOU ON pa.Org_id = HAOU.Organization_id
                   JOIN HR.HR_ALL_ORGANIZATION_UNITS HAOU2 ON PA.Carrying_out_organization_id = HAOU2.Organization_id
                   JOIN apps.gl_code_combinations gcc ON main.Code_Combination_ID_adj = gcc.code_combination_id
          GROUP BY pa.org_id,
                   pa.carrying_out_organization_id,
                   HAOU.NAME,
                   pa.project_type,
                   main.project_id,
                   HAOU2.NAME,
                   pa.segment1,
                   pa.NAME,
                   TRUNC(Trx_date_Adj, 'MM'),
                   main.gl_date,
                   TRUNC(main.gl_date, 'MM'),
                   main.gl_period_name,
                   main.RECEIVABLE_CODE_COMBINATION_ID,
                   RETN_CODE_COMBINATION_ID,
                   Code_Combination_ID_adj,
                   gcc.segment1,
                   gcc.segment2,
                   gcc.segment3,
                   gcc.segment4,
                   gcc.segment5) Main2
         JOIN
         (SELECT HOU2.ORGANIZATION_ID,
                 HOU2.NAME ORG_NAME,
                 SEG1.SEGMENT1,
                 SEG2.SEGMENT2,
                 SEG3.SEGMENT3
          FROM   apps.hr_organization_units hou2,
                 (SELECT hou.organization_id,
                         hou.NAME,
                         psvl.segment_value Segment1
                  FROM   pa.pa_segment_value_lookups psvl,
                         pa.pa_segment_value_lookup_sets psvls,
                         apps.hr_organization_units hou
                  WHERE  psvl.segment_value_lookup_set_id = psvls.segment_value_lookup_set_id
                  AND    psvl.segment_value_lookup = hou.NAME
                  AND    psvls.segment_value_lookup_set_name = 'Comp From Project Org') SEG1,
                 (SELECT hou.organization_id,
                         hou.NAME,
                         psvl.segment_value Segment2
                  FROM   pa.pa_segment_value_lookups psvl,
                         pa.pa_segment_value_lookup_sets psvls,
                         apps.hr_organization_units hou
                  WHERE  psvl.segment_value_lookup_set_id = psvls.segment_value_lookup_set_id
                  AND    psvl.segment_value_lookup = hou.NAME
                  AND    psvls.segment_value_lookup_set_name = 'Group From Project Org') SEG2,
                 (SELECT hou.organization_id,
                         hou.NAME,
                         psvl.segment_value Segment3
                  FROM   pa.pa_segment_value_lookups psvl,
                         pa.pa_segment_value_lookup_sets psvls,
                         apps.hr_organization_units hou
                  WHERE  psvl.segment_value_lookup_set_id = psvls.segment_value_lookup_set_id
                  AND    psvl.segment_value_lookup = hou.NAME
                  AND    psvls.segment_value_lookup_set_name = 'Function From Project Org') SEG3
          WHERE  hou2.organization_id = SEG1.ORGANIZATION_ID(+)
          AND    hou2.organization_id = SEG2.ORGANIZATION_ID(+)
          AND    hou2.organization_id = SEG3.ORGANIZATION_ID(+)
          AND    hou2.internal_external_flag = 'INT'
          AND    HOU2.LOCATION_ID IS NOT NULL) Build_GL ON carrying_out_organization_id = build_GL.ORGANIZATION_ID
GROUP BY org_id,
         Organization_name,
         carrying_out_organization_id,
         Project_Organization_name,
         project_id,
         main2.segment1,
         NAME,
         build_gl.segment1,
         build_gl.segment2,
         build_gl.segment3</property>
