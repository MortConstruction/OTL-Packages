      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{49CAA5DF-0B71-4AEE-A2C3-61E05C038A4F}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="declare @Yyear  Integer&#xA;set @yyear = 2017&#xA;&#xA;/*print @yyear*/&#xA;&#xA;Create Table #CRates (&#xA;period_year  int,&#xA;Period_num  int, &#xA;period_Start_date  datetime,&#xA;period_end_date  datetime,&#xA;functional_currency  Char(10), &#xA;To_currency_code Char(10), &#xA;avg_rate  money, &#xA;eop_rate  money&#xA;)&#xA;Insert into #CRates(&#xA;period_year,&#xA;Period_num, &#xA;period_start_date,&#xA;period_end_date,&#xA;functional_currency, &#xA;To_currency_code, &#xA;avg_rate, &#xA;eop_rate&#xA;)&#xA;select &#xA;YYear&#xA;,1 as PeriodNumber&#xA;,FromDate&#xA;,ToDate&#xA;,From_currency&#xA;,To_Currency&#xA;,PeriodAverageRate&#xA;/*,PeriodAverageInverseRate */&#xA;,PeriodEndRate&#xA;/*,PeriodEndInverseRate */&#xA; &#xA;FROM         OPENQUERY(pmamri, '&#xA; select &#xA;pa.From_currency&#xA;,pa.To_Currency&#xA;,to_char(pa.FromDate,''yyyy'') YYear&#xA;,pa.FromDate&#xA;,pa.ToDate&#xA;,pa.Conversion_Rate as PeriodAverageRate&#xA;,pa.Inverse_Conversion_Rate as PeriodAverageInverseRate&#xA;,pe.Conversion_Rate as PeriodEndRate&#xA;,pe.Inverse_Conversion_Rate as PeriodEndInverseRate&#xA;from&#xA;(select &#xA;gdr.from_currency,&#xA;gdr.to_currency,&#xA;trunc(conversion_date,''mm'') FromDate ,&#xA;gdr.conversion_date as ToDate,&#xA;gdr.conversion_rate,&#xA;gdr.inverse_conversion_rate,&#xA;gdr.conversion_type&#xA;from APPS.GL_Daily_Rates_V gdr&#xA;where 1=1 &#xA;and user_conversion_type like ''Period Average%'' &#xA; and nvl(substr(user_conversion_type,16,1),''x'') &lt;&gt; ''B''&#xA; and nvl(substr(user_conversion_type,12,1),''x'') &lt;&gt; ''B''&#xA;)pa&#xA;LEFT JOIN (&#xA;select  &#xA;&#xA;gdr.from_currency,&#xA;gdr.to_currency,&#xA;trunc(conversion_date,''mm'') FromDate ,&#xA;gdr.conversion_date as ToDate,&#xA;gdr.conversion_rate,&#xA;gdr.inverse_conversion_rate,&#xA;gdr.conversion_type&#xA;from APPS.GL_Daily_Rates_V gdr&#xA;where 1=1 &#xA;and user_conversion_type like ''Period End%''&#xA;and nvl(substr(user_conversion_type,16,1),''x'') &lt;&gt; ''B''&#xA;and nvl(substr(user_conversion_type,12,1),''x'') &lt;&gt; ''B''&#xA;) pe&#xA;on pa.from_currency =pe.from_currency&#xA;and pa.fromdate = pe.fromdate&#xA;and pa.to_currency =pe.to_currency&#xA;&#xA;') Rowset_2&#xA;&#xA;&#xA;&#xA; update #CRates&#xA; set Period_num = MONTH(period_start_date)&#xA; &#xA;/* UPdate EndDate */&#xA; update A&#xA; set a.period_end_date = a.period_end_date+ 10000 &#xA;  /* select period_end_date+ 10000 as ss ,*   */&#xA;  from  #CRates A&#xA;  Inner join&#xA; (select functional_currency,To_currency_code, MAX(period_end_date) as MaxDate&#xA;  from #CRates&#xA; group by functional_currency,to_currency_code) B&#xA; on a.functional_currency = b.functional_currency&#xA; and a.To_currency_code = b.To_currency_code&#xA; and a.period_end_date = b.MaxDate&#xA; &#xA; &#xA;/*select * from #CRates&#xA;where period_year = @yyear /*2012*/&#xA;order by functional_currency,period_Start_date&#xA;*/&#xA;&#xA;delete from&#xA;[Mortenson_Hub].[dbo].[L_CurrencyConversionTable]&#xA;where year(effectiveSTart) &gt;= @yyear&#xA;&#xA;INSERT INTO [Mortenson_Hub].[dbo].[L_CurrencyConversionTable]&#xA;           ([Currency_Code]&#xA;           ,[EffectiveStart]&#xA;           ,[EffectiveEnd]&#xA;           ,[RateToUSD]&#xA;           ,[RateToUSD_BalanceSheet]&#xA;       /*    ,[CreationDate]&#xA;           ,[CreatedBy]&#xA;           ,[LastUpdateDate] */&#xA;           ,[Source])&#xA;&#xA;select &#xA;functional_currency,&#xA;Period_start_date,&#xA;Period_End_Date,&#xA;avg_rate, /* convert(float,1/convert(float,[avg_rate])) as InverseRateTest, */&#xA;eop_rate,&#xA;'oracle' as Source&#xA;from #CRates&#xA;where functional_currency &lt;&gt; 'USD'&#xA;and period_year = @YYear&#xA;&#xA;&#xA;drop table #CRates" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
