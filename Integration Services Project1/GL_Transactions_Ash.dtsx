                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select * from (
 Select 
    natural_Act,
		core.segment1,
		co.company_name,
 		core.segment2,
    gr.group_name,
		core.segment3,
		fu.function_name,
		core.segment4,
		ac.account_name,
		core.segment5,
    InterCo.company_name as InterCompanyName,
core.segment6,
core.segment7,
		core.account_type,
	  gl_amount,
    gl_amount as gl_amount2,
		currency_code,
		je_source,
		je_category,
	  period,   
	  l_date,
    code_combination_id,
    Set_of_books_id,actual_flag 
from (
select 
    gcc.segment1,
 		gcc.segment2,
		gcc.segment3,
		gcc.segment4,
		gcc.segment5,
    gcc.segment6,
    gcc.segment7,
		gjh.currency_code,
		gjh.je_source,
		gjh.je_category,
    gcc.account_type,
    TRUNC(gjl.effective_date, 'MON') as period,   
	   gjl.effective_date l_date,
       to_char(gcc.segment1||'.'||gcc.segment2||'.'||gcc.segment3||'.'||gcc.segment4 ||'.'||gcc.segment5 ||'.'||gcc.segment6) as natural_Act
      ,to_number(gcc.code_combination_id) as code_combination_id
      ,gjh.Ledger_id as Set_of_books_id ,actual_flag 
    /*******Measures*/
	   ,SUM(NVL(gjl.accounted_dr,0)
 	   	- NVL(gjl.accounted_cr,0)) gl_amount 
  from gl.gl_je_headers gjh
 Inner join  gl.gl_je_lines gjl
 on gjh.ledger_id= gjl.ledger_id
 and gjh.je_header_id = gjl.je_header_id
  INNER JOIN    gl.gl_code_combinations gcc 
	 ON gcc.code_combination_id = gjl.code_combination_id 
 where gjl.effective_date &gt;  to_date(20151231,'YYYYMMDD')
 

 /*where gjl.effective_date &gt;  to_date(20101231,'YYYYMMDD')
and gjl.effective_date &lt;  to_date(20120101,'YYYYMMDD')*/
/* Remove later
 and gcc.segment6 &lt;&gt; '0'  */
 Group by
 gcc.segment1,
 		gcc.segment2,
		gcc.segment3,
		gcc.segment4,
		gcc.segment5,
    gcc.segment6,
    gcc.segment7,
		gjh.currency_code,
		je_source,
		je_category,
    gcc.account_type,
    TRUNC(gjl.effective_date, 'MON') ,   
	   gjl.effective_date ,
     to_char(gcc.segment1||'.'||gcc.segment2||'.'||gcc.segment3||'.'||gcc.segment4 ||'.'||gcc.segment5 ||'.'||gcc.segment6), 
     to_number(gcc.code_combination_id) ,
     gjh.Ledger_id,actual_flag
 ) Core
/*******************************************/     
LEFT JOIN (SELECT ffvt.flex_value_meaning Company_No,
       ffvt.Description Company_Name
FROM   apps.fnd_flex_values_tl ffvt
   JOIN apps.fnd_flex_values ffv 
     ON ffvt.flex_value_id = ffv.flex_value_id 
   JOIN apps.fnd_flex_value_sets ffvs
     ON ffv.flex_value_set_id = ffvs.flex_value_set_id
WHERE 1=1  
AND    ffvs.flex_value_set_name = 'MORT_GL_COMPANY') Co
     ON Core.segment1 = CO.Company_No
/*******************************************/
LEFT JOIN (
SELECT ffvt.flex_value_meaning Group_No,
       ffvt.Description Group_Name
FROM   apps.fnd_flex_values_tl ffvt
   JOIN apps.fnd_flex_values ffv 
     ON ffvt.flex_value_id = ffv.flex_value_id 
   JOIN apps.fnd_flex_value_sets ffvs
     ON ffv.flex_value_set_id = ffvs.flex_value_set_id
WHERE 1=1  AND    ffvs.flex_value_set_name = 'MORT_GL_GROUP') Gr
     ON Core.segment2 = Gr.Group_No
/*******************************************/     
LEFT JOIN (SELECT ffvt.flex_value_meaning Function_No,
       ffvt.Description Function_Name
FROM   apps.fnd_flex_values_tl ffvt
   JOIN apps.fnd_flex_values ffv 
     ON ffvt.flex_value_id = ffv.flex_value_id 
   JOIN apps.fnd_flex_value_sets ffvs
     ON ffv.flex_value_set_id = ffvs.flex_value_set_id
WHERE 1=1  AND    ffvs.flex_value_set_name = 'MORT_GL_FUNCTION') Fu
on Core.segment3 = FU.Function_No
/*******************************************/
LEFT JOIN (SELECT ffvt.flex_value_meaning Account_No,
       ffvt.Description Account_Name
FROM   apps.fnd_flex_values_tl ffvt
   JOIN apps.fnd_flex_values ffv 
     ON ffvt.flex_value_id = ffv.flex_value_id 
   JOIN apps.fnd_flex_value_sets ffvs
     ON ffv.flex_value_set_id = ffvs.flex_value_set_id
WHERE 1=1  AND    ffvs.flex_value_set_name = 'MORT_GL_ACCOUNT') Ac
     ON Core.segment4 = AC.Account_No
/*******************************************/     
LEFT JOIN (SELECT ffvt.flex_value_meaning Company_No,
       ffvt.Description Company_Name
FROM   apps.fnd_flex_values_tl ffvt
   JOIN apps.fnd_flex_values ffv 
     ON ffvt.flex_value_id = ffv.flex_value_id 
   JOIN apps.fnd_flex_value_sets ffvs
     ON ffv.flex_value_set_id = ffvs.flex_value_set_id
WHERE 1=1  
AND    ffvs.flex_value_set_name = 'MORT_GL_COMPANY') InterCo
     ON Core.segment5 = InterCo.Company_No
/*******************************************/
/*******************************************/
WHERE 1=1 
and  l_date &gt;  to_date(20151231,'YYYYMMDD')
/*and  l_date &gt;  to_date(20101231,'YYYYMMDD')
and l_date &lt;  to_date(20120101,'YYYYMMDD')*/ ) a
 where a.segment4 is not null</property>
