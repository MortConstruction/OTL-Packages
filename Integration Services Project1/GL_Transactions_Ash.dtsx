          SQLTask:SqlStatementSource=" &#xA; Update t&#xA; set InternalRevenue= internals.CurrentPeriodRevenue&#xA;    ,InternalMargin=Internals.CurrentPeriodMargin&#xA;-- Select gl_periodDate, internals.Project_number,(internals.CurrentPeriodRevenue),(Internals.CurrentPeriodMargin) &#xA;from  MgmtReportingSystem.dbo.js_ExternalJobScheduleMart t&#xA;  INNER JOIN (select GL_PeriodDate,PROJECT_ID,PROJECT_NUMBER,PROJECT_NAME,&#xA; (case when ISNULL(l.GL_EXT2_YTD_BILLED_USD_AMT,0)=0 then l.GL_EXT2_YTD_BILLED_AMT else l.GL_EXT2_YTD_BILLED_USD_AMT end)  * (isnull(l.PA_JV_PCT, 100) / 100) as CurrentPeriodRevenue&#xA; ,(case when ISNULL(l.GL_EXT2_YTD_OM_USD_AMT,0)=0 then l.GL_EXT2_PER_YTD_OPS_MARGIN else l.GL_EXT2_YTD_OM_USD_AMT end) * (isnull(l.PA_JV_PCT, 100) / 100) as CurrentPeriodMargin&#xA; ,l.GL_EXT2_YTD_OM_AMT&#xA; ,l.GL_EXT2_YTD_OM_USD_AMT&#xA; ,l.GL_EXT2_PER_YTD_OPS_MARGIN&#xA; ,   (CASE isnull(GL_EXT2_YTD_BILLED_USD_AMT, 0) WHEN 0 &#xA;        THEN GL_EXT2_YTD_BILLED_AMT ELSE GL_EXT2_YTD_BILLED_USD_AMT END)  * (isnull(l.PA_JV_PCT, 100) / 100) t&#xA;from MgmtReportingSystem.dbo.EFP_HEADER H&#xA;Inner JOin MgmtReportingSystem.dbo.EFP_Lines_Prod L&#xA;on h.EFP_HEADER_ID = l.EFP_HEADER_ID&#xA;where 1=1 --and h.GL_PeriodDate = '12/01/2014'&#xA;) Internals&#xA;on t.queryPeriod=Internals.GL_PeriodDate&#xA;and t.project_id = internals.PROJECT_ID&#xA;--and  t.ledger_id = 6003 &#xA;  and t.project_company not like '%xxxP' --removes dupicates&#xA; and isnull(t.elim_or_not,'N') = 'N'&#xA; --and internals.PROJECT_NUMBER like '07%'&#xA; " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
