                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">/*****Flex Value Name / Description************************/
select ffvv_og.FLEX_VALUE_SET_ID,ffvv_og.flex_value,ffvv_og.description,ffvv_og.enabled_flag,ffvv_og.start_date_active,ffvv_og.end_date_active
 ,CASE WHEN SUBSTR(ffvv_og.COMPILED_VALUE_ATTRIBUTES,5,1) = 'R' THEN
