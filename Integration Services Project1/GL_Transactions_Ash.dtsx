          SendMailTask:MessageSource="INSERT INTO [Mort_EIS].[dbo].[M_Project_MasterGroupingItems]&#xA;           ([Project_GroupingKey]&#xA;           ,[ProjectID]&#xA;&#x9;&#x9;&#x9;,[ProjectNameOverride]&#xA;           ,[SortNumber]&#xA;           ,[ItemNote]&#xA;)&#xA;select&#xA;21 as Project_GroupingKey,  &#xA;p.project_id,&#xA;p.project_name,&#xA;convert(int,left(p.Project_number,8)) as SortNumber,&#xA;'SLT Book Group Sort -- OG' as ItemNote&#xA;-- a.*, p.Project_name, p.project_number&#xA; from Mort_EIS.dbo.fExecOraRpt_ProjectSorts(1)  a&#xA;INNER JOIN mortenson_hub.dbo.r_projects p&#xA;on a.projectid = p.project_id&#xA;where parentGroupingKey &lt;&gt; 2&#xA;and p.project_id &lt;&gt; 1616&#xA;and p.project_id not in (&#xA;select projectid from Mort_EIS.dbo.fExecOraRpt_ProjectSorts(17) &#xA;)" xmlns:SendMailTask="www.microsoft.com/sqlserver/dts/tasks/sendmailtask" />
