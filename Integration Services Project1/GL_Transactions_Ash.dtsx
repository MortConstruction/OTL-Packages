          SQLTask:SqlStatementSource="Update js&#xA;set js.incomeFromContractsPriorYear = convert(money,py.IncomeFromContract)&#xA;--,js.IncomeFromContractsCurrentYear = convert(money,js.IncomeFromContract - isnull(py.IncomeFromContract,0))&#xA;--from&#xA;/*select  &#xA;js.Ledger_id&#xA;,js.QueryPeriod&#xA;,js.QuerySource&#xA;,js.Project_id&#xA;,js.project_number&#xA;,js.Project_Company&#xA;,js.GroupSegment&#xA;,js.IncomeFromContract&#xA;,js.IncomeFromContractsCurrentYear&#xA;,py.IncomeFromContract&#xA;,js.incomeFromContractsPriorYear&#xA;,js.IncomeFromContract - isnull(py.IncomeFromContract,0) YearIncome&#xA;--,py.IncomeFromContractsCurrentYear&#xA;*/&#xA;from dbo.JS_ExternalJobScheduleMart JS&#xA;INNER JOIN (select  &#xA;Ledger_id&#xA;--,DATEPART(YYYY,queryperiod)-1 as t&#xA;,QueryPeriod&#xA;,QuerySource&#xA;,Project_id&#xA;,Project_Company&#xA;,GroupSegment&#xA;,IncomeFromContract&#xA;,IncomeFromContractsCurrentYear&#xA;from dbo.JS_ExternalJobScheduleMart JS&#xA;where 1=1 /*and Project_number = '11030016'*/ and month(QueryPeriod)=12) py&#xA;on py.Project_id = js.project_id&#xA;and py.ledger_id = js.ledger_id&#xA;and year(py.queryperiod)+1=YEAR(js.QueryPeriod)&#xA;and py.querySource = JS.querySource&#xA;and py.Project_Company = JS.Project_Company&#xA;and py.GroupSegment = JS.GroupSegment&#xA;where 1=1 /*and Project_number = '11030016'*/&#xA;--Order by js.project_number,js.QueryPeriod&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
