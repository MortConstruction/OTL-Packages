        <SQLTask:SqlTaskData
          SQLTask:Connection="{49CAA5DF-0B71-4AEE-A2C3-61E05C038A4F}"
          SQLTask:SqlStatementSource="/**********Pass 1 Same as Pass 2 with an additional where ******************************************************/&#xA;Update  FVN   --select top 10 * from Mortenson_Hub.dbo.SCD_FlexValueNames FVN&#xA;set js_ApplicationClusterName = base.FLEX_VALUE&#xA;--select *&#xA;from&#xA;(select FLEX_VALUE_SET_ID,FLEX_VALUE,DESCRIPTION as Flex_Value_Description,FLIP_SIGN_FLAG,sfvn.js_ApplicationClusterName,sfvn.js_ApplicationCluster&#xA;from Mortenson_Hub.dbo.SCD_FlexValueNames sfvn&#xA;INNER JOIN (select distinct AccountName from MgmtReportingSystem.dbo.GL_AccountApplicationGroup) AAG&#xA;on AAG.AccountName = sfvn.FLEX_VALUE  &#xA;--and sfvn.FLEX_VALUE_SET_ID = 1009750&#xA;where 1=1 and sfvn.EffectiveEndDate is null&#xA;) Base&#xA;INNER Join Mortenson_Hub.dbo.SCD_ChildValues Child&#xA;on base.FLEX_VALUE_SET_ID = child.Flex_value_Set_id&#xA;and Child.Parent_Flex_value = base.FLEX_VALUE&#xA;INNER JOIN Mortenson_Hub.dbo.SCD_FlexValueNames FVN&#xA;on fvn.FLEX_VALUE_SET_ID = child.FLEX_VALUE_SET_ID&#xA;and fvn.FLEX_VALUE = child.Flex_value&#xA;--where Base.FLEX_VALUE = 'Revenue'&#xA;where 1=1&#xA;/*2 Passes 1 with where clause below second without it**/&#xA;and parent_flex_value in ('Billing','EFREVADJ') -- Get Sub Type First (Duplicate Revenue includes Revenue and Billing -- Cost includes Costs and EFREVADJ&#xA;and fvn.js_ApplicationClusterName is null&#xA;&#xA;/**********Pass 2 Same as Pass 1 with an additional where ******************************************************/&#xA;Update  FVN   --select top 10 * from Mortenson_Hub.dbo.SCD_FlexValueNames FVN&#xA;set js_ApplicationClusterName = base.FLEX_VALUE&#xA;--select *&#xA;from&#xA;(select FLEX_VALUE_SET_ID,FLEX_VALUE,DESCRIPTION as Flex_Value_Description,FLIP_SIGN_FLAG,sfvn.js_ApplicationClusterName,sfvn.js_ApplicationCluster&#xA;from Mortenson_Hub.dbo.SCD_FlexValueNames sfvn&#xA;INNER JOIN (select distinct AccountName from MgmtReportingSystem.dbo.GL_AccountApplicationGroup) AAG&#xA;on AAG.AccountName = sfvn.FLEX_VALUE  &#xA;--and sfvn.FLEX_VALUE_SET_ID = 1009750&#xA;where 1=1 and sfvn.EffectiveEndDate is null&#xA;) Base&#xA;INNER Join Mortenson_Hub.dbo.SCD_ChildValues Child&#xA;on base.FLEX_VALUE_SET_ID = child.Flex_value_Set_id&#xA;and Child.Parent_Flex_value = base.FLEX_VALUE&#xA;INNER JOIN Mortenson_Hub.dbo.SCD_FlexValueNames FVN&#xA;on fvn.FLEX_VALUE_SET_ID = child.FLEX_VALUE_SET_ID&#xA;and fvn.FLEX_VALUE = child.Flex_value&#xA;--where Base.FLEX_VALUE = 'Revenue'&#xA;where 1=1&#xA;/*2 Passes 1 with where clause below second without it**/&#xA;-------and parent_flex_value in ('Billing','EFREVADJ') -- Get Sub Type First (Duplicate Revenue includes Revenue and Billing -- Cost includes Costs and EFREVADJ&#xA;and fvn.js_ApplicationClusterName is null&#xA;&#xA;/***Pass 3 Marks Segments that are not parents******************************/&#xA;Update sfvn &#xA;set js_ApplicationClusterName = aag.AccountName&#xA;--select * &#xA;from Mortenson_Hub.dbo.SCD_FlexValueNames sfvn&#xA;INNER JOIN &#xA;   (select distinct AccountName from MgmtReportingSystem.dbo.GL_AccountApplicationGroup) AAG&#xA;  on sfvn.FLEX_VALUE = aag.AccountName&#xA;and sfvn.js_ApplicationClusterName is null&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
