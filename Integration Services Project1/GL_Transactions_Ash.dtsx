        <SQLTask:SqlTaskData
          SQLTask:Connection="{49CAA5DF-0B71-4AEE-A2C3-61E05C038A4F}"
          SQLTask:SqlStatementSource="Create Table #CRates (&#xA;period_year  int,&#xA;Period_num  int, &#xA;period_Start_date  datetime,&#xA;period_end_date  datetime,&#xA;functional_currency  Char(10), &#xA;To_currency_code Char(10), &#xA;avg_rate  money, &#xA;eop_rate  money&#xA;)&#xA;Insert into #CRates(&#xA;period_year,&#xA;Period_num, &#xA;period_start_date,&#xA;period_end_date,&#xA;functional_currency, &#xA;To_currency_code, &#xA;avg_rate, &#xA;eop_rate&#xA;)&#xA;select &#xA;period_year,&#xA;Period_num, &#xA;starter,&#xA;period_end_date,&#xA;functional_currency, &#xA;To_currency_code, &#xA;avg_rate, &#xA;eop_rate&#xA;FROM         OPENQUERY(pmamri, '&#xA;  select &#xA;period_year,&#xA;Period_num, &#xA;period_end_date,&#xA;functional_currency, &#xA;To_currency_code, &#xA;avg_rate, &#xA;eop_rate,&#xA;nvl((select max(Period_end_date) +1 base&#xA;from apps.GL_TRANSLATION_RATES_V i &#xA;where i.functional_currency=r.functional_currency&#xA; and i.period_end_date &lt; r.period_end_date  ),(period_end_date)) as starter&#xA;  from apps.GL_TRANSLATION_RATES_V R&#xA;  where period_year = 2012&#xA;') Rowset_2&#xA;&#xA;delete from&#xA;[Mortenson_Hub].[dbo].[L_CurrencyConversionTable]&#xA;where year(effectiveSTart) &gt;= 2012&#xA;&#xA;INSERT INTO [Mortenson_Hub].[dbo].[L_CurrencyConversionTable]&#xA;           ([Currency_Code]&#xA;           ,[EffectiveStart]&#xA;           ,[EffectiveEnd]&#xA;           ,[RateToUSD]&#xA;           ,[RateToUSD_BalanceSheet]&#xA;       --    ,[CreationDate]&#xA;       --    ,[CreatedBy]&#xA;       --    ,[LastUpdateDate]&#xA;           ,[Source])&#xA;select &#xA;functional_currency,&#xA;Period_start_date,&#xA;Period_End_Date,&#xA;avg_rate,&#xA;eop_rate,&#xA;'oracle' as Source&#xA;from #CRates&#xA;&#xA;&#xA;update c&#xA;set effectiveEnd = eEnd+10000&#xA;--select *&#xA;from [Mortenson_Hub].[dbo].[L_CurrencyConversionTable] c&#xA;INNER JOIN &#xA;(select currency_code, max(effectiveStart) eStart, max(effectiveEnd) eEnd&#xA; from [Mortenson_Hub].[dbo].[L_CurrencyConversionTable]&#xA;where year(effectiveSTart) &gt;= 2012&#xA;group by currency_code) F&#xA;on c.currency_code = f.currency_code&#xA;and c.EffectiveStart = f.EStart&#xA;and c.EffectiveEnd = f.EEnd&#xA;&#xA;drop Table #CRates" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
