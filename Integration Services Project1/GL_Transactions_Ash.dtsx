      ,ffvv_og.End_date_active /*, ffvv_og.**/
          from  apps.fnd_flex_values_vl ffvv_og
           Inner join apps.fnd_flex_value_sets ffvs_og
           on ffvv_og.flex_value_set_id =  ffvs_og.flex_value_set_id
           /***********/
          inner join ( select  distinct
  idfs.flex_value_set_id  /*,idfs.application_column_name*/
  FROM apps.gl_sets_of_books glsob
  INNER JOIN apps.fnd_id_flex_segments idfs
  on glsob.chart_of_accounts_id = idfs.id_flex_num
  where 1=1 
  AND idfs.application_id = 101 
AND idfs.id_flex_code = 'GL#'
  and idfs.application_column_name = 'SEGMENT1' /** COMPANY SEGMENT**/) fSet
  on ffvv_og.flex_value_set_id = fset.flex_value_set_id
                           where 1=1
                           and ffvs_og.flex_value_set_name = 'MORT_GL_COMPANY'
                            and nvl(ffvv_og.attribute1,'N')='Y'</property>
