                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select 
B.Ledger_id
,B.Code_Combination_id
,B.period_name
,to_date('20' || (substr(period_name,5,2)) || substr(period_name,1,3) || '01','YYYYMONDD' ) Period_Date
/****Segment Flex Values*****/
,segment1
,segment2
,segment3
,segment4
,segment5
,segment6
,b.Actual_Flag
,b.period_Num
,b.period_Year
,b.currency_code
,gcc.account_type
,sum(B.period_net_dr-B.period_net_cr) CurrentPeriodBalance
,sum(B.begin_balance_dr - B.begin_balance_cr) PeriodBegBalance
/*,sum(B.begin_balance_dr - B.begin_balance_cr) AProjectToDateBalance*/
,sum(nvl(PROJECT_TO_DATE_DR,0)- nvl(PROJECT_TO_DATE_CR,0)) ProjectToDateBalance
from apps.gl_balances  B
INNER JOIN gl.gl_code_combinations GCC
on B.Code_Combination_id = GCC.Code_Combination_id
where 1=1
and period_year =2015
and period_num = 6
/* and period_num = 12*/

/*(period_num = 1 
or period_year &gt;=2014)*/
/*and segment4='EXRVFRST'
and segment6 = '07090005'
and ledger_id = 6003
and code_combination_id in (select code_combination_id from gl.gl_code_combinations where segment4 in ('143000','147000')) */
group by 
B.Ledger_id
,B.Code_Combination_id
,B.period_name
,segment1
,segment2
,segment3
,segment4
,segment5
,segment6
,b.Actual_Flag
,b.period_Num
,b.period_Year
,b.currency_code
,gcc.account_type
having sum(B.begin_balance_dr - B.begin_balance_cr) &lt;&gt; 0 
or sum(nvl(PROJECT_TO_DATE_DR,0)- nvl(PROJECT_TO_DATE_CR,0)) &lt;&gt; 0
or sum(B.period_net_dr-B.period_net_cr)  &lt;&gt; 0
