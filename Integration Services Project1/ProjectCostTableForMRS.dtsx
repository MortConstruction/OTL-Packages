                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">/*3 UNION Queries -- Costs,Revenue,Units*/
select project_id,task_id,expenditure_type,gl_month
,sum(Billings) as Billings
,sum(acct_burdened_cost) as  ActualCost
,sum(ActualUnits) as ActualUnits
,sum(hours) as Hours
from
(
SELECT /*, pcdl.expenditure_item_id,*/
                                     pcdl.project_id
                                     , pcdl.task_id
                                     ,pei.expenditure_type
                                     , (trunc(pcdl.gl_date, 'MONTH')) gl_month
                                     ,0 as Billings
                                     ,sum(pcdl.acct_burdened_cost) acct_burdened_cost
                                     ,0 as ActualUnits
                                     ,sum(CASE
           WHEN pet.unit_of_measure = 'HOURS' 
        AND    pcdl.line_type IN('R', 'D')
      /*  AND    x.incurred_by_person_id IS NOT NULL*/
        AND    (    NVL(pcdl.reversed_flag, 'N') != 'Y'
                OR pcdl.line_num_reversed IS NULL)
              THEN NVL(pcdl.quantity, 0)
           ELSE 0.00
        END) as Hours
                            FROM apps.pa_cost_distribution_lines_all pcdl
                            INNER JOIN apps.pa_expenditure_items_all pei
                            on pcdl.expenditure_item_id = pei.expenditure_item_id
 INNER JOIN apps.pa_expenditure_types pet 
                            on pet.expenditure_type = pei.expenditure_type
                           WHERE 
                           1=1  
                           and pcdl.gl_date&lt;= To_date('06302013','MMddYYYY')
                           /*and   pcdl.project_id in (11823,59699,77699)     */
                  and pcdl.project_id in (select distinct  project_id
from pa.pa_projects_all
where segment1 like '13%')
                           GROUP BY  pcdl.project_id
                           ,(trunc(pcdl.gl_date, 'MONTH')) 
                           ,pei.expenditure_type
                           /*pcdl.expenditure_item_id*/
                           , pcdl.task_id
 /****************************************************/
/**Revenue/Billings**/
UNION ALL
select
pei.project_id
,pcdl.task_id
,pei.expenditure_type
 ,trunc(pei.expenditure_item_date,'month') as  gl_month
 ,sum(NVL(pei.bill_amount,0)) actual_revenue
 ,0 as acct_burdened_cost
 ,0 as ActualUnits
,0 as Hours
from apps.pa_expenditure_items_all pei
Inner join (select distinct expenditure_item_id,task_id from apps.pa_cost_distribution_lines_all a 
            where /* a.project_id in (11823,59699,77699) */
1=1                  
and a.project_id in (select distinct  project_id
from pa.pa_projects_all
where segment1 like '13%')
and a.gl_date&lt;= To_date('06302013','MMddYYYY')) pcdl
on pcdl.expenditure_item_id = pei.expenditure_item_id
inner join apps.pa_expenditure_types pet
on pet.expenditure_type = pei.expenditure_type
where 1=1 
/*and pei.project_id in (11823,59699,77699)*/
                  and pei.project_id in (select distinct  project_id
from pa.pa_projects_all
where segment1 like '13%')
and (NVL(pei.bill_amount,0))  &lt;&gt; 0
group by pei.project_id 
,pcdl.task_id
,pei.expenditure_type
 ,trunc(pei.expenditure_item_date,'month')
/****************************************************/                           
/*********Units****************/
UNION ALL
select  
pap3.project_id 
,otl.task_id
, otl.expenditure_type
,trunc(otl.timecard_entry_date,'month') as  gl_month
  /*,pet.expenditure_type */
,0  as Billings
,0 as ActualCost
,sum(NVL(otl.units, 0) )  as JTD_ActualUnits
,0 as Hours
          FROM apps.xxotl_timecard_v otl
              INNER JOIN pa.pa_projects_all pap3
              on   otl.project_id = pap3.project_id
               AND otl.approval_status &lt;&gt; 'ERROR'
               INNER JOIN  (select Project_id from pa.pa_projects_all
where 1=1
/*and project_id in (11823,59699,77699)*/
                  and project_id in (select distinct  project_id
from pa.pa_projects_all
where segment1 like '13%')

) lst
on lst.project_id = pap3.project_id
where 1=1 
and otl.timecard_entry_date &lt; to_date('20130630','yyyymmdd')
group by 
pap3.project_id 
,otl.task_id
,otl.timecard_entry_date
,otl.expenditure_type
having sum(NVL(otl.units, 0) ) &lt;&gt; 0   
 )
 group by 
  project_id,task_id,expenditure_type,gl_month</property>
