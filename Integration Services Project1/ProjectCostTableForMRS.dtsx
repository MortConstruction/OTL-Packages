                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select  
pap3.project_id 
,otl.task_id
/*--,sysdate as baselined_date
--,otl.timecard_entry_date */
,-1 as budget_version_id 
, 'XY' as budget_type_code
, sysdate as baselined_date
,'N'  as current_original_flag
,'Actual' as version_name
,'Find' as expenditure_category
, otl.expenditure_type  /*,pet.expenditure_type */
/*****Measures  ********/
,0 OriginalCostBudget /*Query 1*/
,0 as OriginalEstimateUnits /*Query 1*/

,0 as OriginalRevenueBudget /*Query 2*/

,0  as CurrentCostBudget
,0  as CurrentCostBudgetUnits

,0 as CurrentCostRevenueBudget

,0 as OriginalForecastBudget
,0 as OriginalForecastBudgetUnits
,0 as OriginalForecastRevenue

,0 as CurrentForecastCostBudget
,0  as CurrentForecastCostBudgetUnits
,0 as CurrentForecastRevenue

,0 as JTD_ActualCosts
,sum(NVL(otl.units, 0) )  as JTD_ActualUnits


          FROM apps.xxotl_timecard_v otl
              INNER JOIN pa.pa_projects_all pap3
              on   otl.project_id = pap3.project_id
               AND otl.approval_status &lt;&gt; 'ERROR'
               INNER JOIN  (select Project_id from pa.pa_projects_all
where 1=1
/*and project_id  in (11823,59699,77699)*/
 and project_id in (select distinct  project_id
from pa.pa_projects_all
where segment1 like '13%')
/*and carrying_out_organization_id = 238
and segment1 like '11%' */) lst
on lst.project_id = pap3.project_id
where 1=1 
and otl.timecard_entry_date &lt; to_date('20130630','yyyymmdd')
group by 
pap3.project_id 
,otl.task_id
,otl.timecard_entry_date
,otl.expenditure_type
having sum(NVL(otl.units, 0) ) &lt;&gt; 0</property>
