                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select pa.org_id
	   ,pa.carrying_out_organization_id
	   ,HAOU.Name Organization_Name
	   ,pa.project_type
	   ,main.project_id
	   ,HAOU2.Name Project_Organization_Name
	   ,pa.segment1
	   ,pa.name
	   ,main.expenditure_type
	   /*,main.transaction_source */
     , null as transaction_source
	   ,trunc(main.trx_date,'MM') Trx_date
	   ,main.Trx_date_Adj
	   ,trunc(main.expenditure_item_date,'MM') expenditure_item_date
	   ,sum(Actual_cost) Monthly_Actual_Cost
     ,sum(Total_Hours) as Project_Hours
	   ,main.gl_period_name
       ,main.dr_code_combination_id
	   ,gcc.segment1 segment1_COMPANY
       ,gcc.segment2 segment2_GROUP
       ,gcc.segment3 segment3_FUNCTION
       ,gcc.segment4 segment4_ACCOUNT
,gcc.segment5 segment5_InterCompany
from 
 ( SELECT pei.project_id,
         pet.expenditure_type,
   	     pei.transaction_source,
         pcdl.gl_date trx_date,
		 pei.expenditure_item_date,
         0 orig_rev_budget,
         0 curr_rev_budget,
         0 actual_revenue,
         0 est_rev_completion,
         0 orig_cost_budget,
         0 curr_cost_budget,
         SUM (NVL(pcdl.acct_burdened_cost,0)) actual_cost,
         0 committed_cost,
         0 int_pending_cost,
         0 est_cost_completion,
         0 ext_pending_rev,
         0 ext_pending_cost,
         0 budget_transfer,
         0 estimated_qty,
         NULL estimated_uom,
         0 actual_qty,
         6 row_type
		 ,pcdl.gl_period_name
		 ,pcdl.dr_code_combination_id
		 ,case when pa_period_name = 'DEC-05'and gl_period_name &lt;&gt; 'DEC-05' and transaction_source = 'ALLOCATIONS' then to_date('01-DEC-2005','DD-MON-YYYY') else trunc(pcdl.gl_date,'MM') end Trx_date_Adj
, sum(CASE
           WHEN pet.unit_of_measure = 'HOURS'
        AND    pcdl.line_type IN('R', 'D')
        AND    x.incurred_by_person_id IS NOT NULL
        AND    (    NVL(pcdl.reversed_flag, 'N') != 'Y'
                OR pcdl.line_num_reversed IS NULL)
              THEN NVL(pcdl.quantity, 0)
           ELSE 0.00
        END) Total_Hours

FROM pa.pa_expenditure_types pet,
         apps.pa_cost_distribution_lines_all pcdl,
          apps.pa_expenditures_all x,
         apps.pa_expenditure_items_all pei
   WHERE pei.expenditure_item_id = pcdl.expenditure_item_id
     AND pet.expenditure_type = pei.expenditure_type
      and  x.expenditure_id = pei.expenditure_id
   /*  and pei.project_id = 6239 */
   and pcdl.gl_date &gt;= to_date('01-Jan-2015','DD-MON-YYYY') 
  /* and pcdl.gl_date &gt;= to_date('01-Jan-2009','DD-MON-YYYY')*/ 
  /* and pcdl.gl_date &lt;= to_date('31-DEC-2013','DD-MON-YYYY') */
GROUP BY pei.project_id
         ,pet.expenditure_type
		 ,pei.transaction_source
		 ,pei.expenditure_item_date
    	 ,pcdl.gl_date
    	 ,pcdl.gl_period_name
		 ,pcdl.dr_code_combination_id
		 ,case when pa_period_name = 'DEC-05'and gl_period_name &lt;&gt; 'DEC-05' and transaction_source = 'ALLOCATIONS' then to_date('01-DEC-2005','DD-MON-YYYY') else trunc(pcdl.gl_date,'MM') end 
) MAIN
  	   JOIN pa.pa_projects_all pa
		on main.project_id = pa.project_id
/*    and pa.project_id = 6239 */
	   JOIN HR.HR_ALL_ORGANIZATION_UNITS HAOU
	   			ON pa.Org_id = HAOU.Organization_id
	   JOIN HR.HR_ALL_ORGANIZATION_UNITS HAOU2
	   			ON PA.Carrying_out_organization_id = HAOU2.Organization_id
	   JOIN apps.gl_code_combinations gcc
	            ON main.dr_code_combination_id = gcc.code_combination_id
group by
	    pa.org_id
	   ,pa.carrying_out_organization_id
	   ,HAOU.Name 
   	   ,pa.project_type
	   ,main.project_id
	   ,HAOU2.Name 
	   ,pa.segment1
	   ,pa.name
	   ,main.gl_period_name
	   ,main.dr_code_combination_id
	   ,gcc.segment1 
     ,gcc.segment2 
     ,gcc.segment3 
    ,gcc.segment4 
    ,gcc.segment5 
	   ,main.expenditure_type
	/*   ,main.transaction_source */
	   ,trunc(main.trx_date,'MM') 
	   ,trunc(main.expenditure_item_date,'MM')
	   ,main.Trx_date_Adj</property>
