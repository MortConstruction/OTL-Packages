Declare @MonthNumber  Int
Declare @YearNumber  Int
set @MonthNumber = 18
set @YearNumber = year(getdate())-- 2010

select distinct
Project_ID,
expenditure_type,
EndDate,
case EndDate when DATEADD(mm, -@MonthNumber,dbo.GetLastDateOfMonth(DATEADD(DAY, DATEDIFF(DAY, 0, GETDATE()), 0))) then -1 else 0 end as BaseMonth
--,dbo.GetLastDateOfMonth(Getdate()) as LastDay
--,DATEADD(DAY, DATEDIFF(DAY, 0, GETDATE()), 0) 
--,DATEADD(mm, -12,dbo.GetLastDateOfMonth(DATEADD(DAY, DATEDIFF(DAY, 0, GETDATE()), 0)))
 from
(select distinct
project_id,
expenditure_type 
from dbo.fExecEISBook_ConstructionProjectInfo()
where 1=1
and project_id in (
select distinct project_id
from Mortenson_Hub.dbo.R_Projects p 
where year(Start_Date) &lt;= year(GETDATE()) and year(p.Completion_Date) &gt;=year(GETDATE())-1
and Project_Type = 'Direct'
/*select distinct Oracle_Project_id 
from mortenson_hub.dbo.MRS_MRSJBMR_Stage
where 1=1 
and left(jbperd,4) = @YearNumber --'2010'
union
select distinct project_id
from dbo.fExecEISBook_ConstructionProjectInfo()
where year(Trx_Date) = @YearNumber --2010
and row_type &lt;&gt; 999
*/
)
and year(Trx_Date) &lt;= year(getdate())
--and year(Trx_Date) &lt;= year(a.Trx_Date)
group by
Project_id
,expenditure_type ) A
cross join
(select EndDate,dbo.GetLastDateOfMonth(Getdate()) as PeriodDate 
--,DATEADD(mm, DATEDIFF(mm,0,@reportDate), 0)
--DATEADD(mm, DATEDIFF(mm,0,dbo.GetLastDateOfMonth(Getdate())), 0)
--,DATEADD(mm, -12,dbo.GetLastDateOfMonth(Getdate()))
 from b_Dim_MonthSeries
Where EndDate between DATEADD(mm, -@MonthNumber,dbo.GetLastDateOfMonth(Getdate())) and dbo.GetLastDateOfMonth(Getdate())
) d</property>
