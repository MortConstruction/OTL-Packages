/*(NVL(TO_NUMBER(ltrim(rtrim(pbl.attribute1))), 0)) Units */
/*,(NVL(pbl.revenue, 0)) - Revenue */

,(NVL(pbl.burdened_cost,0))   OriginalCostBudget
,(NVL(TO_NUMBER(ltrim(rtrim(pbl.attribute1))), 0)) as OriginalEstimateUnits
,0 as OriginalRevenueBudget /*Query 2*/

,0 as CurrentCostBudget
,0 as CurrentCostBudgetUnits
,0 as CurrentCostRevenueBudget

,0 as OriginalForecastBudget
,0 as OriginalForecastBudgetUnits
,0 as OriginalForecastRevenue

,0 as CurrentForecastCostBudget
,0 as CurrentForecastCostBudgetUnits
,0 as CurrentForecastRevenue

,0 as JTD_ActualCosts
,0 as JTD_ActualUnits
,(CASE  WHEN upper(pbl.attribute2) = 'HOURS'
                             THEN NVL(pbl.quantity, 0)
                          ELSE 0
                       END) hours_estimated                       

FROM   pa.pa_budget_versions pbv   
INNER JOIN pa.pa_budget_lines pbl
 on pbl.budget_version_id = pbv.budget_version_id
  inner JOIN        pa.pa_resource_assignments pra
  on pbl.resource_assignment_id = pra.resource_assignment_id
  inner JOIN         pa.pa_resource_list_members prlm
on    prlm.resource_list_member_id = pra.resource_list_member_id 
   inner JOIN pa.pa_expenditure_types pet 
 on pet.expenditure_type = prlm.alias
 INNER JOIN          (SELECT v1.Project_id,
                 v1.Budget_version_id
          FROM   (SELECT   pa.project_id,
                           MAX(NVL(c.budget_version_id, 0)) version_id
                  FROM     pa.pa_budget_versions c
                      inner join 
                           pa.pa_projects_all pa
                           on pa.project_id = c.project_id
                  WHERE    1 = 1
             /*     and pa.project_id  in (11823,59699,77699)  */
 and pa.project_id in (select distinct  project_id
from pa.pa_projects_all
where segment1 like '13%')
                  AND      pa.project_type &lt;&gt; 'Department'
                  AND      c.budget_status_code = 'B'
                  AND      c.budget_type_code = 'AC'
                  AND      c.current_original_flag = 'Y'
                  GROUP BY pa.project_id) lst
                  INNER JOIN 
                 pa.pa_budget_versions v1
                 on lst.project_id = v1.project_id 
                 AND    lst.version_id = v1.Budget_version_id
          WHERE  1=1
         ) b1
on b1.project_id = pbv.project_id
and b1.Budget_version_id = pbv.budget_version_id
where 1=1</property>
