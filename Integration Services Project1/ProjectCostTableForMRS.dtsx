    <DTS:Executable
      DTS:refId="Package\Update All Other Months"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{81F2C7D9-0F87-4196-A8AA-D2BC9BDC8EF0}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Update All Other Months"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server v9;  2004 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="3">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{84C09E50-0536-41FD-B4CC-C847F94A0D08}"
          SQLTask:SqlStatementSource="/* STEP 3 Make loop for all months  */&#xA;&#xA;--Make loop&#xA;Declare @MonthNumber  Int&#xA;set @MonthNumber = 17&#xA;&#xA;while @MonthNumber &lt;&gt; -1&#xA;begin&#xA;&#xA;&#xA;&#xA;--Declare @MonthNumber  Int&#xA;--set @MonthNumber = 11&#xA;&#xA;Update S&#xA;set Cost_JTD = n.JTDPeriodCosts&#xA;,ProjectHours_JTD = n.JTDPeriodProjectHours&#xA;,Cost_CurrentMonth = n.CurrentPeriodCosts&#xA;,Cost_PrevMonth =  n.PreviousPeriodCosts&#xA;,Cost_YTD = YTDPeriodCosts&#xA;,ProjectHours_CurrentMonth = n.CurrentPeriodProjectHours&#xA;,ProjectHours_PrevMonth = n.PreviousPeriodProjectHours&#xA;,ProjectHours_YTD = n.YTDPeriodProjectHours&#xA;,Bill_JTD = n.JTDRevenue&#xA;,Bill_YTD = n.YTDPeriodBillings&#xA;,Bill_CurrentMonth = n.CurrentPeriodBillings&#xA;,Bill_PrevMonth = n.PreviousPeriodBillings&#xA;&#xA;--select n.*&#xA;from&#xA;(&#xA;&#xA;select &#xA;project_id,&#xA;expenditure_type,&#xA;trx_date,&#xA;--- JOB TO DATE&#xA;--Sum(JTDPeriodCosts) as JTDPeriodCosts -- not necessary because of union&#xA;Sum(JTDPeriodCosts) as JTDPeriodCosts&#xA;--,Sum(PeriodRevenueAmount)&#xA;,Sum(JTDPeriodProjectHours) as JTDPeriodProjectHours&#xA;,Sum(JTDPeriodRevenueAmount) as JTDRevenue&#xA;-- CURRENT PERIOD&#xA;,sum(CurrentPeriodCosts) as CurrentPeriodCosts&#xA;--,Sum(PeriodRevenueAmount)&#xA;,sum(CurrentPeriodProjectHours) as CurrentPeriodProjectHours&#xA;,sum(CurrentPeriodRevenueAmount) as CurrentPeriodBillings&#xA;-- Previous PERIOD&#xA;,sum(PreviousPeriodCosts) as PreviousPeriodCosts&#xA;--,Sum(PeriodRevenueAmount)&#xA;,sum(PreviousPeriodProjectHours) as PreviousPeriodProjectHours&#xA;,sum(PreviousPeriodRevenueAmount) as PreviousPeriodBillings&#xA;--- YTD&#xA;,sum(YTDPeriodCosts) as YTDPeriodCosts&#xA;--,Sum(PeriodRevenueAmount)&#xA;,sum(YTDPeriodProjectHours) as YTDPeriodProjectHours&#xA;,sum(YTDRevenueAmount) as YTDPeriodBillings&#xA;from (&#xA;select &#xA;--min(dbo.GetLastDateOfMonth(Trx_date)) as MinTrx_Month,&#xA;--max(dbo.GetLastDateOfMonth(Trx_date)) as MaxTrx_Month,&#xA;project_id,&#xA;expenditure_type,&#xA;dbo.GetLastDateOfMonth(DATEADD(mm, -@MonthNumber,dbo.GetLastDateOfMonth(Getdate()))) as trx_date&#xA;--- JOB TO DATE&#xA;--,Sum(PeriodCosts) as JTDPeriodCosts&#xA;,sum(case dbo.GetLastDateOfMonth(Trx_date) when dbo.GetLastDateOfMonth(DATEADD(mm, -@MonthNumber,Getdate())) then  (PeriodCosts) else 0 end) as JTDPeriodCosts&#xA;--,Sum(PeriodRevenueAmount)&#xA;--,Sum(PeriodProjectHours) JTDPeriodProjectHours&#xA;,sum(case dbo.GetLastDateOfMonth(Trx_date) when dbo.GetLastDateOfMonth(DATEADD(mm, -@MonthNumber,Getdate())) then  (PeriodProjectHours) else 0 end ) JTDPeriodProjectHours&#xA;,sum(case dbo.GetLastDateOfMonth(Trx_date) when dbo.GetLastDateOfMonth(DATEADD(mm, -@MonthNumber,Getdate())) then  (PeriodRevenueAmount) else 0 end ) JTDPeriodRevenueAmount&#xA;-- CURRENT PERIOD&#xA;,sum(case dbo.GetLastDateOfMonth(Trx_date) when dbo.GetLastDateOfMonth(DATEADD(mm, -@MonthNumber,Getdate())) then  (PeriodCosts) else 0 end) as CurrentPeriodCosts&#xA;--,Sum(PeriodRevenueAmount)&#xA;,sum(case dbo.GetLastDateOfMonth(Trx_date) when dbo.GetLastDateOfMonth(DATEADD(mm, -@MonthNumber,Getdate())) then  (PeriodProjectHours) else 0 end ) as CurrentPeriodProjectHours&#xA;,sum(case dbo.GetLastDateOfMonth(Trx_date) when dbo.GetLastDateOfMonth(DATEADD(mm, -@MonthNumber,Getdate())) then  (PeriodRevenueAmount) else 0 end ) as CurrentPeriodRevenueAmount&#xA;-- Previous PERIOD&#xA;--,sum(case dbo.GetLastDateOfMonth(Trx_date) when DATEADD(mm, -(@MonthNumber+1),dbo.GetLastDateOfMonth(Getdate())) then  (PeriodCosts) else 0 end) as PreviousPeriodCosts&#xA;,0 as PreviousPeriodCosts&#xA;--,sum(case dbo.GetLastDateOfMonth(Trx_date) when DATEADD(mm, -(@MonthNumber+1),dbo.GetLastDateOfMonth(Getdate())) then  (PeriodProjectHours) else 0 end ) as PreviousPeriodProjectHours&#xA;,0 as PreviousPeriodProjectHours&#xA;,0 as PreviousPeriodRevenueAmount&#xA;--- YTD&#xA;--,sum(case Year(dbo.GetLastDateOfMonth(Trx_date)) when Year(DATEADD(mm, -@MonthNumber,dbo.GetLastDateOfMonth(Getdate()))) then  (PeriodCosts) else 0 end) as YTDPeriodCosts&#xA;,sum(case dbo.GetLastDateOfMonth(Trx_date) when dbo.GetLastDateOfMonth(DATEADD(mm, -@MonthNumber,Getdate())) then  (PeriodCosts) else 0 end)  as YTDPeriodCosts&#xA;--,Sum(PeriodRevenueAmount)&#xA;--,sum(case Year(dbo.GetLastDateOfMonth(Trx_date)) when Year(DATEADD(mm, -@MonthNumber,dbo.GetLastDateOfMonth(Getdate()))) then  (PeriodProjectHours) else 0 end ) as YTDPeriodProjectHours&#xA;,sum(case dbo.GetLastDateOfMonth(Trx_date) when dbo.GetLastDateOfMonth(DATEADD(mm, -@MonthNumber,Getdate())) then  (PeriodProjectHours) else 0 end ) as YTDPeriodProjectHours&#xA;,sum(case dbo.GetLastDateOfMonth(Trx_date) when dbo.GetLastDateOfMonth(DATEADD(mm, -@MonthNumber,Getdate())) then  (PeriodRevenueAmount) else 0 end ) as YTDRevenueAmount&#xA;from dbo.fExecEISBook_ConstructionProjectInfo()&#xA;where 1=1&#xA;--and year(dbo.GetLastDateOfMonth(Trx_date)) = 2009&#xA;and (dbo.GetLastDateOfMonth(Trx_date)) = dbo.GetLastDateOfMonth(DATEADD(mm, -@MonthNumber,dbo.GetLastDateOfMonth(Getdate())))&#xA;--and project_id = 42&#xA;group by&#xA;--dbo.GetLastDateOfMonth(Trx_date) ,&#xA;--DATEADD(mm, -12,dbo.GetLastDateOfMonth(Getdate())),&#xA;project_id,&#xA;expenditure_type&#xA;--,(dbo.GetLastDateOfMonth(Trx_date))&#xA;--order by 1,2,3&#xA;&#xA;Union All&#xA;---Union back JTD from Previous Month (this will avoid any issues of no costs/budgets in current month)&#xA;--Declare @MonthNumber  Int&#xA;--set @MonthNumber = 12&#xA;select &#xA;ProjectID as Project_ID,&#xA;ExpenditureType as Expenditure_type,&#xA;--(dbo.GetLastDateOfMonth(Trx_date)) as trx_date&#xA;dbo.GetLastDateOfMonth(DATEADD(mm, -(@MonthNumber+0),dbo.GetLastDateOfMonth(Getdate()))) as trx_date&#xA;--- JOB TO DATE&#xA;,(Cost_JTD) as JTDPeriodCosts&#xA;--,Sum(PeriodRevenueAmount)&#xA;,(ProjectHours_JTD) JTDPeriodProjectHours&#xA;,Bill_JTD as JTDRevenueAmount&#xA;-- CURRENT PERIOD&#xA;,0 as CurrentPeriodCosts&#xA;--,Sum(PeriodRevenueAmount)&#xA;,0 as CurrentPeriodProjectHours&#xA;,0 as CurrentPeriodRevenueAmount&#xA;-- Previous PERIOD&#xA;,Cost_CurrentMonth as PreviousPeriodCosts&#xA;--,Sum(PeriodRevenueAmount)&#xA;,ProjectHours_CurrentMonth as PreviousPeriodProjectHours&#xA;,Bill_CurrentMonth as PreviousPeriodRevenueAmount&#xA;--- YTD&#xA;,(case Year(dbo.GetLastDateOfMonth(Trx_date)) when Year(DATEADD(mm, -(@MonthNumber + 0),dbo.GetLastDateOfMonth(Getdate()))) then  (Cost_YTD) else 0 end) as YTDPeriodCosts&#xA;--,Sum(PeriodRevenueAmount)&#xA;,(case Year(dbo.GetLastDateOfMonth(Trx_date)) when Year(DATEADD(mm, -(@MonthNumber+0),dbo.GetLastDateOfMonth(Getdate()))) then  (ProjectHours_YTD) else 0 end ) as YTDPeriodProjectHours&#xA;,(case Year(dbo.GetLastDateOfMonth(Trx_date)) when Year(DATEADD(mm, -(@MonthNumber+0),dbo.GetLastDateOfMonth(Getdate()))) then  (Bill_YTD) else 0 end ) as YTDRevenueAmount&#xA;from mort_eis.dbo.CM_DirectFactSummary_Stage&#xA;where 1=1&#xA;--and year(dbo.GetLastDateOfMonth(Trx_date)) = 2009&#xA;and (dbo.GetLastDateOfMonth(Trx_date)) = dbo.GetLastDateOfMonth(DATEADD(mm, -(@MonthNumber+1),dbo.GetLastDateOfMonth(Getdate())))&#xA;) sub&#xA;group by&#xA;project_id,&#xA;expenditure_type,&#xA;trx_date&#xA;/* */&#xA;--order by Project_ID,expenditure_type&#xA;) &#xA;n&#xA;&#xA;INNER JOIN dbo.CM_DirectFactSummary_Stage S&#xA;ON s.ProjectID= n.Project_id&#xA;and s.expendituretype = n.expenditure_type&#xA;and s.trx_date = n.trx_date&#xA;--order by 1,2,3&#xA;&#xA;&#xA;select @monthNumber&#xA;set @monthNumber = @monthNumber -1&#xA;end&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
