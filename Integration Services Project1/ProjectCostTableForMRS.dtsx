        <SQLTask:SqlTaskData
          SQLTask:Connection="{84C09E50-0536-41FD-B4CC-C847F94A0D08}"
          SQLTask:SqlStatementSource="/* STEP 2 Populate Initial Job To Date Costs */&#xA;Declare @MonthNumber  Int&#xA;set @MonthNumber = 18&#xA;&#xA;Update S&#xA;set Cost_JTD = n.JTDPeriodCosts&#xA;,ProjectHours_JTD = n.JTDPeriodProjectHours&#xA;,Bill_JTD = n.JTDRevenue&#xA;,Bill_YTD = n.YTDPeriodBillings&#xA;,Bill_CurrentMonth = n.CurrentPeriodBillings&#xA;,Bill_PrevMonth = n.PreviousPeriodBillings&#xA;,Cost_CurrentMonth = n.CurrentPeriodCosts&#xA;,Cost_PrevMonth =  n.PreviousPeriodCosts&#xA;,Cost_YTD = YTDPeriodCosts&#xA;,ProjectHours_CurrentMonth = n.CurrentPeriodProjectHours&#xA;,ProjectHours_PrevMonth = n.PreviousPeriodProjectHours&#xA;,ProjectHours_YTD = n.YTDPeriodProjectHours&#xA;&#xA;--Select * &#xA;from&#xA;(select &#xA;--min(dbo.GetLastDateOfMonth(Trx_date)) as MinTrx_Month,&#xA;--max(dbo.GetLastDateOfMonth(Trx_date)) as MaxTrx_Month,&#xA;project_id,&#xA;expenditure_type,&#xA;dbo.GetLastDateOfMonth(DATEADD(mm, -@MonthNumber,dbo.GetLastDateOfMonth(Getdate()))) as trx_date&#xA;--- JOB TO DATE&#xA;,Sum(PeriodCosts) as JTDPeriodCosts&#xA;--,Sum(PeriodRevenueAmount)&#xA;,Sum(PeriodProjectHours) as JTDPeriodProjectHours&#xA;,Sum(PeriodRevenueAmount) as JTDRevenue&#xA;-- CURRENT PERIOD&#xA;,sum(case dbo.GetLastDateOfMonth(Trx_date) when dbo.GetLastDateOfMonth(DATEADD(mm, -@MonthNumber,Getdate())) then  (PeriodCosts) else 0 end) as CurrentPeriodCosts&#xA;--,Sum(PeriodRevenueAmount)&#xA;,sum(case dbo.GetLastDateOfMonth(Trx_date) when dbo.GetLastDateOfMonth(DATEADD(mm, -@MonthNumber,Getdate())) then  (PeriodProjectHours) else 0 end ) as CurrentPeriodProjectHours&#xA;,sum(case dbo.GetLastDateOfMonth(Trx_date) when dbo.GetLastDateOfMonth(DATEADD(mm, -@MonthNumber,Getdate())) then  (PeriodRevenueAmount) else 0 end ) as CurrentPeriodBillings&#xA;-- Previous PERIOD&#xA;,sum(case dbo.GetLastDateOfMonth(Trx_date) when dbo.GetLastDateOfMonth(DATEADD(mm, -(@MonthNumber+1),Getdate())) then  (PeriodCosts) else 0 end) as PreviousPeriodCosts&#xA;--,Sum(PeriodRevenueAmount)&#xA;,sum(case dbo.GetLastDateOfMonth(Trx_date) when dbo.GetLastDateOfMonth(DATEADD(mm, -(@MonthNumber+1),Getdate())) then  (PeriodProjectHours) else 0 end ) as PreviousPeriodProjectHours&#xA;,sum(case dbo.GetLastDateOfMonth(Trx_date) when dbo.GetLastDateOfMonth(DATEADD(mm, -(@MonthNumber+1),Getdate())) then  (PeriodRevenueAmount) else 0 end ) as PreviousPeriodBillings&#xA;--- YTD&#xA;,sum(case Year(dbo.GetLastDateOfMonth(Trx_date)) when dbo.GetLastDateOfMonth(Year(DATEADD(mm, -@MonthNumber,Getdate()))) then  (PeriodCosts) else 0 end) as YTDPeriodCosts&#xA;--,Sum(PeriodRevenueAmount)&#xA;,sum(case Year(dbo.GetLastDateOfMonth(Trx_date)) when dbo.GetLastDateOfMonth(Year(DATEADD(mm, -@MonthNumber,Getdate()))) then  (PeriodProjectHours) else 0 end ) as YTDPeriodProjectHours&#xA;,sum(case Year(dbo.GetLastDateOfMonth(Trx_date)) when dbo.GetLastDateOfMonth(Year(DATEADD(mm, -@MonthNumber,Getdate()))) then  (PeriodRevenueAmount) else 0 end ) as YTDPeriodBillings&#xA;from dbo.fExecEISBook_ConstructionProjectInfo()&#xA;where 1=1&#xA;--and year(dbo.GetLastDateOfMonth(Trx_date)) = 2009&#xA;and (dbo.GetLastDateOfMonth(Trx_date)) &lt;= dbo.GetLastDateOfMonth(DATEADD(mm, -@MonthNumber,dbo.GetLastDateOfMonth(Getdate())))&#xA;--and project_id = 7612&#xA;group by&#xA;--dbo.GetLastDateOfMonth(Trx_date) ,&#xA;--DATEADD(mm, -12,dbo.GetLastDateOfMonth(Getdate())),&#xA;project_id,&#xA;expenditure_type&#xA;--,(dbo.GetLastDateOfMonth(Trx_date))&#xA;--order by 1,2,3) &#xA;)N&#xA;INNER JOIN dbo.CM_DirectFactSummary_Stage S&#xA;ON s.ProjectID= n.Project_id&#xA;and s.expendituretype = n.expenditure_type&#xA;and s.trx_date = n.trx_date&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
