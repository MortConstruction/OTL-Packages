                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select 
pcdl.project_id,
p.segment1 as ProjectNumber,
p.name as ProjectName,
pcdl.task_id,
t.task_number,
t.task_name,
t.parent_task_id,
t.top_task_id,
to_number(pet.attribute5) expenditure_sort,
pet.expenditure_type_id,
pet.expenditure_category,
pei.expenditure_type,
pcdl.line_num AS PCDL_LineNumber,
pcdl.gl_date trx_date,
pei.NON_LABOR_RESOURCE,
pei.ORIG_TRANSACTION_REFERENCE,
pei.Job_id,
x.incurred_by_person_id,
n.full_name,
pv.segment1 AS VendorNumber,
pv.vendor_name AS VendorName,
pei.po_line_id,
 TO_NUMBER(x.ORIG_EXP_TXN_REFERENCE1) AP_Invoice_ID,
nvl(AP.INVOICE_NUM,ipt.IPT_Invoice) as INVOICE_NUM,
PEI.attribute8 as IPT_AP_Invoice_NUM,
AP.ATTRIBUTE1 APPLICATION_NUMBER,
AP.ATTRIBUTE2 SOV_NUMBER,
AP.ATTRIBUTE3 SOV_PROJECT_NUMBER,
PEC.EXPENDITURE_COMMENT,
 /*Measure*/
(NVL(pcdl.acct_burdened_cost, 0)) actual_cost,
/* GET Hours */
       (CASE
           WHEN pet.unit_of_measure = 'HOURS'
        AND    pcdl.line_type IN('R', 'D')
        AND    x.incurred_by_person_id IS NOT NULL
        AND    (    NVL(pcdl.reversed_flag, 'N') != 'Y'
                OR pcdl.line_num_reversed IS NULL)
              THEN NVL(pcdl.quantity, 0)
           ELSE 0.00
        END) Total_Hours
from apps.pa_cost_distribution_lines_all pcdl
INNER JOIN apps.pa_expenditure_items_all pei 
ON pei.expenditure_item_id = pcdl.expenditure_item_id
INNER JOIN apps.pa_expenditures_all x 
		ON x.expenditure_id = pei.expenditure_id
INNER JOIN apps.pa_expenditure_types pet 
		ON pet.expenditure_type = pei.expenditure_type
INNER JOIN pa.pa_projects_all p
on p.project_id = pcdl.project_id
INNER JOIN pa.pa_tasks t
on t.project_id = pcdl.project_id
and t.task_id = pcdl.task_id
/*OUTER JOINS  (IPT use invoice number which is not unique -- this is a problem)*/
LEFT OUTER JOIN 
(select invoice_num, count(*) as issue,
'IPT inv: ' || invoice_num || '(' || max(pv.segment1) || '-'|| max(pv.vendor_name) || ')' as IPT_Invoice
from APPS.AP_INVOICES_ALL AP 
INNER JOIN  APPS.AP_SUPPLIERS pv ON ap.vendor_id = PV.vendor_id
where 1=1 group by invoice_num) ipt
on ipt.invoice_num = pei.attribute8 
LEFT OUTER JOIN APPS.AP_SUPPLIERS pv ON x.vendor_id = PV.vendor_id
LEFT OUTER JOIN APPS.AP_INVOICES_ALL AP ON TO_NUMBER(x.ORIG_EXP_TXN_REFERENCE1) = AP.INVOICE_ID
LEFT OUTER JOIN apps.pa_expenditure_comments pec ON PEC.EXPENDITURE_ITEM_ID = PEI.EXPENDITURE_ITEM_ID
       LEFT OUTER JOIN
       (SELECT   person_id,
                 MIN(full_name) Full_Name
        FROM     apps.per_all_people_f
        GROUP BY person_id) N ON n.person_id = x.incurred_by_person_id
WHERE  1 = 1 
and (NVL(pcdl.acct_burdened_cost, 0)) &lt;&gt; 0 
and   pcdl.project_id in (379929,380032,379954,380109,380281,380232
,354717,
380357,
380423,
380273,
380298,
380181,
380777,
380038
,380131
,379928
,380427
,380429
)
/*AND pei.expenditure_type = ''Subcontracts''*/
AND pei.expenditure_type in ('Subcontracts','Materials Contracts','Professional Services','Purchased Materials')
and pcdl.gl_date between TO_DATE('01012005','ddmmyyyy') and TO_DATE('31012019','ddmmyyyy')</property>
