                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select 
     p.segment1
 , h.sub_sov_number|| d.line_number as  origin_id
    ,v.vendor_name
    ,h.sub_sov_number
    ,d.line_number
    ,h.sub_sov_date
    ,h.status
    ,h.version
    ,h.inactive_flag
    ,h.description header_description
    ,d.retention_percentage 
    ,h.last_update_date Header_Last_Update
    ,t.task_number 
    ,t.task_name 
    ,e.expenditure_type  
    ,d.quantity
    ,d.description line_description
    ,d.last_update_date Line_Last_Update
from apps.ence_subsov_hdr_all h
        , apps.ence_subsov_dtl_all d
        , apps.pa_projects_all p
        , apps.pa_tasks t
        , apps.pa_expenditure_types e
        , apps.ap_suppliers v
        , (select project_id, sub_sov_number, max(sub_sov_id) as sub_sov_id from apps.ence_subsov_hdr_all group by project_id, sub_sov_number) m
   where p.project_id= h.project_id
        and m.sub_sov_id = h.sub_sov_id
        and h.sub_sov_id = d.sub_sov_id
        and d.task_id = t.task_id
        and d.expenditure_type_id= e.expenditure_type_id
        and h.vendor_id = v.vendor_id
        and p.project_status_code &lt;&gt; 'CLOSED'
        and h.status = 'APPROVED' 
    order by segment1</property>
