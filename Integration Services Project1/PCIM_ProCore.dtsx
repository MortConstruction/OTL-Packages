     when nvl(epdsa.Sub_Estimate,-999.882) &lt;&gt; -999.882 then epdsa.Sub_Estimate
     when  nvl(epdsa.rom_Estimate, -999.882) &lt;&gt; -999.882 then nvl(epdsa.Rom_Estimate,0)
   else 0
   end ) as SelectedAmount

,max(case when nvl(epdsa.SubCo_Estimate,-999.882) &lt;&gt; -999.882 then 'No'
     when nvl(epdsa.Sub_Estimate,-999.882) &lt;&gt; -999.882 then 'No'
     when  nvl(epdsa.rom_Estimate, -999.882) &lt;&gt; -999.882 then 'No'
   else 'Yes'
   end ) as SelectedAmountIsNull
   
/*, sum(case when nvl(epdsa.SubCo_Estimate,-999.882) =-999.882 then nvl(epdsa.SubCo_Estimate,0)
