                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select
10 as RecordSource
,esh.project_id
,esd.expenditure_type_id
,esh.sub_sov_id
,esh.sub_sov_number
,esh.description
,'Original Contract' as CHANGE_ORDER_REASON_DESC
,esh.sub_sov_date as Sub_SOV_Action_Date
,esh.Vendor_id 
,esh.VENDOR_SITE_ID
,pv.segment1 as Vendor_No
,pv.vendor_name as Vendor_Name
,esh.attribute7 as SDI_Enrolled
,pv.Minority_group_lookup_code  
,(esd.price_per_unit * esd.quantity) as exp_tot_contract_amt
,0.00 exp_tot_invoice_amt
,0.00 exp_tot_retained
,0.00 as ChangeOrders
,0.00 as MamChangeOrders
,0.00 as Total_amount_PAID 
,0.00 as Retainage_amount_PAID
,0.00 as Regular_amount_PAID
 FROM apps.ence_subsov_hdr_all esh
  INNER JOIN apps.ence_subsov_dtl_all esd
  on  esh.sub_sov_id = esd.sub_sov_id
  INNER JOIN apps.po_vendors pv
  on pv.vendor_id = esh.vendor_id
/*inner join apps.pa_expenditure_types pet 
on pet.expenditure_type_id = esd.expenditure_type_id*/
  /*****************/
where 1=1  /*and rownum &lt; 10*/
 AND      esh.status = 'APPROVED'
 and esh.project_id  in (379929,380032,379954,380109,380281,380232
,354717,
380357,
380423,
380273,
380298,
380181,
380777
,380038
,380131
,379928
,380427
,380429
)
 /*and (esh.Project_id in (' + @ProjectList + ') or -1 = ' + @ProjectAll + ' )
 and esh.sub_sov_date between To_Date(''''' + @StartDate + ''''',''''ddmmyyyy'''') and To_Date(''''' + @EndDateTEXT + ''''',''''ddmmyyyy'''')*/
 and esh.version = (select nvl(min(esh2.version),1) from apps.ence_subsov_hdr esh2
 where esh2.status = 'APPROVED'
 and esh2.project_id = esh.project_id
 and esh2.sub_sov_number = esh.sub_sov_number)
 union all
 select 20 as RecordSource /*Change Order*/
,esh.project_id
,ecod.expenditure_type_id 
,min(esh.sub_sov_id) as sub_sov_id
,esh.sub_sov_number
,esh.description
,ecoh.CHANGE_ORDER_REASON_DESC
,ecoh.change_order_date as Original_Sub_SOV_Date
,esh.Vendor_id
,esh.VENDOR_SITE_ID
,pv.segment1 as Vendor_No
,pv.vendor_name as Vendor_Name
,esh.attribute7 as SDI_Enrolled
,pv.Minority_group_lookup_code    
,0 exp_tot_contract_amt
,0 exp_tot_invoice_amt
,0 exp_tot_retained
,SUM(decode(ecoh.change_order_reason_code, 'BUDGET TRANSFER', 0, round(ecod.incremental_change_value,2))) as ChangeOrders
,SUM(decode(ecoh.change_order_reason_code, 'BUDGET TRANSFER', round(ecod.incremental_change_value,2), 0)) as MamChangeOrders
,0 as Total_amount_PAID 
,0 as Retainage_amount_PAID
,0 as Regular_amount_PAID
    from apps.ence_subcosov_hdr_all ecoh
        INNER JOIN apps.ence_subcosov_dtl_all ecod
        on  ecoh.subco_sov_id = ecod.subco_sov_id
        INNER JOIN apps.ence_subsov_hdr_all esh
        on esh.sub_sov_id = ecoh.sub_sov_id
        INNER JOIN apps.pa_expenditure_types pet_co
           on pet_co.expenditure_type_id = ecod.expenditure_type_id
        INNER JOIN apps.pa_projects_all pp
        on esh.project_id = pp.project_id
        INNER JOIN apps.po_vendors pv
        on esh.vendor_id = pv.vendor_id
   where 1=1 
      and ecoh.status = 'APPROVED'
     and esh.status = 'APPROVED'
     and esh.project_id  in (379929,380032,379954,380109,380281,380232
,354717,
380357,
380423,
380273,
380298,
380181,
380777
,380038
,380131
,379928
,380427
,380429)
	   /*and (esh.Project_id in (' + @ProjectList + ') or -1 = ' + @ProjectAll + ' )
	   and ecoh.change_order_date between To_Date(''''' + @StartDate + ''''',''''ddmmyyyy'''') and To_Date(''''' + @EndDateTEXT + ''''',''''ddmmyyyy'''')*/
 group by esh.project_id
,ecod.expenditure_type_id
,esh.sub_sov_id
,esh.sub_sov_number
,esh.description
,ecoh.CHANGE_ORDER_REASON_DESC
,ecoh.change_order_date 
,esh.Vendor_id
,esh.VENDOR_SITE_ID
,pv.segment1
,pv.vendor_name 
,esh.attribute7
,pv.Minority_group_lookup_code  
union all
 select
30 as RecordSource
,esh.project_id
,-1 as expenditure_type_id
,esh.sub_sov_id
,esh.sub_sov_number
,esh.description
,esah.application_number
,esah.application_date as Original_Sub_SOV_Date
,esh.Vendor_id
,esh.VENDOR_SITE_ID
,pv.segment1 as Vendor_No
,pv.vendor_name as Vendor_Name
,esh.attribute7 as SDI_Enrolled
,pv.Minority_group_lookup_code    
,0 as exp_tot_contract_amt
,(nvl(esad.amount_to_be_billed,0)) exp_tot_invoice_amt
,(nvl(esad.amount_to_be_retained,0)) exp_tot_retained
,0 as ChangeOrders
,0 as MamChangeOrders
,0 as Total_amount_PAID 
,0 as Retainage_amount_PAID
,0 as Regular_amount_PAID  
 FROM     apps.ence_subapplsov_hdr_all esah
INNER JOIN apps.ence_subapplsov_dtl_all esad
on esah.subappl_sov_id = esad.subappl_sov_id
INNER JOIN apps.ence_subsov_hdr_all esh
on esah.sub_sov_id = esh.sub_sov_id
INNER JOIN apps.po_vendors pv
  on pv.vendor_id = esh.vendor_id
where 1=1 
 and esh.project_id  in (379929,380032,379954,380109,380281,380232
,354717,
380357,
380423,
380273,
380298,
380181,
380777
,380038
,380131
,379928
,380427
,380429
)
/*and esah.application_date between To_Date(' + @StartDate + ','ddmmyyyy') and To_Date(' + @EndDateTEXT + ','ddmmyyyy')
and (esh.Project_id in (' + @ProjectList + ') or -1 = ' + @ProjectAll + ' )*/
 AND      esad.amount_complete IS NOT NULL
 AND      esah.status = 'APPROVED'
union all
SELECT 40 as RecordSource
,aid.project_id
,-1 as expenditure_type_id
,-1 as sub_sov_id
,ai.attribute2 as sub_sov_number
,'a' as description
,ai.attribute1 as CHANGE_ORDER_REASON_DESC
,ai.gl_date as Original_Sub_SOV_Date
,ai.Vendor_id
,ai.VENDOR_SITE_ID
,pv.segment1 as Vendor_No
,pv.vendor_name as Vendor_Name
,null as SDI_Enrolled
,pv.Minority_group_lookup_code  
,0 exp_tot_contract_amt
,0 exp_tot_invoice_amt
,0 exp_tot_retained
,0 as ChangeOrders
,0 as MamChangeOrders 
,(NVL(aid.amount,0)) as Total_amount_PAID 
,(case instr(ai.invoice_num,'RET') when 0 then 0 else  NVL(aid.amount,0) end) as  Retainage_amount_PAID
,(case instr(ai.invoice_num,'RET') when 0 then NVL(aid.amount,0) else  0 end) as Regular_amount_PAID
FROM apps.ap_invoices_all ai
  INNER JOIN apps.ap_invoice_distributions_all aid
         on ai.invoice_id = aid.invoice_id
  INNER JOIN apps.po_vendors pv
  on pv.vendor_id = ai.vendor_id       
  WHERE aid.line_type_lookup_code != 'TAX'
 and aid.project_id  in (379929,380032,379954,380109,380281,380232,354717,
380357,
380423,
380273,
380298,
380181,
380777
,380038
,380131
,379928
,380427
,380429
)
 	   /*and (aid.Project_id in (' + @ProjectList + ') or -1 = ' + @ProjectAll + ' )*/
    and ai.attribute1 is not null
     AND EXISTS (SELECT 'X'
     FROM   apps.ap_invoice_payments_all aip
    WHERE  1=1
    and aip.invoice_id = ai.invoice_id 
    /*AND aip.accounting_date between To_Date(''''' + @StartDate + ''''',''''ddmmyyyy'''') and To_Date(''''' + @EndDateTEXT + ''''',''''ddmmyyyy''''))*/
    and nvl(reversal_flag, 'N') != 'Y')</property>
