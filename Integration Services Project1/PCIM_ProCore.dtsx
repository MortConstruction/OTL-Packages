    <DTS:Executable
      DTS:refId="Package\Clean-up"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{19A17215-2203-484B-9768-33A72F7B85A2}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Clean-up"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="1">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{F42EA6B7-EC00-4BE5-AB35-6735F7737F0E}"
          SQLTask:SqlStatementSource="update u&#xA;set expenditure_type_id=maxExpend&#xA;--select * &#xA;from&#xA; (select Sub_Sov_number, expenditure_type_id &#xA;  from Mortenson_Hub.dbo.T_Subcontractor_SOV_Stage&#xA;  where expenditure_type_id = -1) u&#xA;inner join &#xA;(select Sub_Sov_number, expenditure_type_id as maxExpend,CurrentAmount &#xA;,ROW_NUMBER() over(partition by Sub_Sov_number order by currentAmount desc )as rrow&#xA;from&#xA;(select  Sub_Sov_number, expenditure_type_id&#xA;, sum(s.EXP_TOT_CONTRACT_AMT + s.changeorders + mamchangeorders) as CurrentAmount&#xA;  from Mortenson_Hub.dbo.T_Subcontractor_SOV_Stage S&#xA;  where expenditure_type_id &lt;&gt; -1&#xA;  group by Sub_Sov_number, expenditure_type_id &#xA;  )  a  ) f&#xA;  on u.sub_sov_number = f.sub_sov_number&#xA;  where rrow=1&#xA;&#xA;/***************************/&#xA;Update U&#xA;set Sub_Sov_id  = f.Sub_Sov_id &#xA;--select * &#xA;from&#xA;(select  Sub_Sov_number, Sub_Sov_id &#xA;  from Mortenson_Hub.dbo.T_Subcontractor_SOV_Stage&#xA;  where sub_Sov_id = -1) U&#xA;Inner join&#xA;(select distinct Sub_Sov_number, Sub_Sov_id &#xA;  from Mortenson_Hub.dbo.T_Subcontractor_SOV_Stage&#xA;  where sub_Sov_id &lt;&gt; -1) F&#xA;on u.Sub_Sov_number = f.Sub_Sov_number&#xA;&#xA;/*********************/&#xA;update s&#xA;set EXPENDITURE_TYPE = t.EXPENDITURE_TYPE&#xA;--select t.EXPENDITURE_TYPE,s.expenditure_type &#xA;from Mortenson_Hub.dbo.R_pa_expenditure_types t&#xA;inner join Mortenson_Hub.[dbo].T_Subcontractor_SOV_Stage s&#xA;on t.expenditure_type_id = s.expenditure_type_id&#xA;where s.expenditure_type is null&#xA;&#xA;/******Update SDI Enrolled flag on payments based on SOV**************************************/&#xA;  update s&#xA;  set SDI_Enrolled = MaxSDI_Enrolled&#xA;  --select s.SUB_SOV_ID, s.SDI_Enrolled, b.MaxSDI_Enrolled&#xA;  from mortenson_hub.dbo.T_Subcontractor_SOV_Stage s&#xA;  inner join (&#xA;  select SUB_SOV_ID, max(SDI_Enrolled) as MaxSDI_Enrolled&#xA;     from mortenson_hub.dbo.T_Subcontractor_SOV_Stage s&#xA;  group by SUB_SOV_ID) b&#xA;  on b.SUB_SOV_ID=s.SUB_SOV_ID&#xA;&#xA;/***********updated sdi enrolled flag in job cost*************/&#xA;  update jc&#xA;  set sdi_enrolled = u.sdi_enrolled&#xA; -- select jc.SOV_NUMBER,jc.sdi_enrolled,u.sdi_enrolled &#xA;  from mortenson_hub.dbo.T_Subcontractor_SOV_JobCost_Stage jc&#xA;  inner join &#xA;  (select distinct SUB_SOV_NUMBER,sdi_enrolled &#xA;   from mortenson_hub.dbo.T_Subcontractor_SOV_Stage s&#xA;   where s.sdi_enrolled is not null) u&#xA;   on u.SUB_SOV_NUMBER = jc.sov_number &#xA;  where jc.SOV_NUMBER is not null" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
