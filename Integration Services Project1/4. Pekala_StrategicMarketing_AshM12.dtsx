                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">/*Remove the individual group bys -- add variable for period*/
select p.segment1 as ProjectNumber
    ,mr.project_id
    , pfp.pfp_year_month
    , mr.package_num
    , mr.lumpsum_package_flag
    , mr.category_name
    , round(nvl(mr.antcp_rev_budget_amt,0),2) as  antcp_rev_budget_amt
    , round(nvl(mr.fac_rev_amt,0 ),2) as  fac_rev_amt
    , round(nvl(mr.fac_cost_amt,0),2) as  fac_cost_amt
    , round(nvl(mr.margin_risk_amt,0),2) as  margin_risk_amt
    , round(nvl(mr.gmp_overrun,0),2) as  gmp_overrun
,0 as ARB
,0 as FR
from apps.xxpa_pfp_margin_risk_form_v mr /*apps.xxpa_pfp_margin_risk_v mr*/
    , apps.pa_projects_all p
    , (select project_id, pfp_year_month, max(pfp_header_id) as maxpfp from apps.xxpa_pfp_header_all
               where status_code = 'SUBMITTED' and type_code = 'PFP' group by project_id, pfp_year_month) pfp 
where mr.project_id = p.project_id 
    and pfp.maxpfp = mr.pfp_header_id
/*order by p.segment1, pfp.pfp_year_month*/

union all
select 
    min(p.segment1) as ProjectNumber
    ,mr.project_id
    , pfp.pfp_year_month
    , min(mr.package_num)
    , min(mr.lumpsum_package_flag)
    , 'GMP_Overrun' as category_name
    , 0  as antcp_rev_budget_amt
    ,  0 as fac_rev_amt
    , 0 as fac_cost_amt
    , 0 as margin_risk_amt
    , round(sum(nvl(mr.gmp_overrun,0)),2) as gmp_overrun

/*,sum(case when mr.package_num is not null and mr.lumpsum_package_flag = 'N' then round(nvl(mr.antcp_rev_budget_amt,0),2) else round(nvl(mr.antcp_rev_budget_amt,0),2) end) arb
    ,sum(case when mr.package_num is not null and mr.lumpsum_package_flag = 'N' then round(nvl(mr.antcp_rev_budget_amt,0),2) else round(nvl(mr.fac_rev_amt,0),2) end) FR *//************************************/
,sum(case when mr.package_num is not null and mr.lumpsum_package_flag = 'N' then round(nvl(mr.antcp_rev_budget_amt,0),2) else round(nvl(mr.antcp_rev_budget_amt,0),2) end) arb3
    ,sum(case when mr.package_num is not null and mr.lumpsum_package_flag = 'N' and mr.antcp_rev_budget_amt&lt;mr.fac_rev_amt  then round(nvl(mr.antcp_rev_budget_amt,0),2) else round(nvl(mr.fac_rev_amt,0),2) end) FR3 /*mr.antcp_rev_budget_amt*/


/*mr.antcp_rev_budget_amt*/

    /*mr.project_id
    , pfp.pfp_year_month
    , sum(mr.gmp_overrun) gmp_overrun*/
/********************************************************************************/
from apps.xxpa_pfp_margin_risk_form_v mr  /*apps.xxpa_pfp_margin_risk_v mr*/
    , apps.pa_projects_all p
    , (select project_id, pfp_year_month, max(pfp_header_id) as maxpfp from apps.xxpa_pfp_header_all
               where status_code = 'SUBMITTED' and type_code = 'PFP' group by project_id, pfp_year_month) pfp 
where mr.project_id = p.project_id
    and pfp.maxpfp = mr.pfp_header_id
   /* and nvl(mr.gmp_overrun,0) &lt;&gt; 0*/
group by 
mr.project_id
    , pfp.pfp_year_month</property>
