              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select  HAOU.NAME	ORGANIZATION
      ,haou.organization_id  
       ,pp.project_id
       ,PP.NAME 		PROJECT_NAME
       ,HAO.NAME	 	PROJECT_ORGANIZATION_NAME
        ,HAO.organization_id as Carrying_Out_Organization_ID
       ,PP.segment1                   PROJECT_NO
       ,HP.party_name               CUSTOMER
       ,HP.party_number                CUSTOMER_NUMBER
       ,RCT.trx_number              INVOICE_NUMBER
       ,RCT.customer_trx_id      TRX_ID
       ,RCT.trx_date                  INVOICE_DATE
       ,APS.due_date                DUE_DATE
       ,APS.gl_date                   GL_DATE
       ,trunc(aps.gl_date_closed)    GL_Date_Closed
       ,aps.status
      ,aps.class
       --'**MEASURES*******************************
       ,nvl(aps.amount_due_original,0) amount_due_original
       ,nvl(aps.amount_due_remaining,0) amount_due_remaining
from apps.pa_projects_all          PP
Inner join apps.ra_customer_trx_all      RCT
  on RCT.interface_header_attribute1 = PP.segment1 
INNER JOIN apps.ar_payment_schedules_all APS
on RCT.customer_trx_id = APS.customer_trx_id
INNER JOIN apps.hz_cust_accounts     HCA
on    HCA.cust_account_id = APS.customer_id
INNER JOIN  apps.hz_parties           HP
on   HCA.party_id = HP.party_id
INNER JOIN apps.hr_operating_units    HAOU
on HAOU.organization_id = PP.org_id
INNER JOIN apps.hr_all_organization_units HAO
on HAO.organization_id = PP.carrying_out_organization_id
where 1=1 
/*--and  HAO.organization_id = 223 /*194*/  /*and haou.organization_id = 84 */
--and RCT.trx_number in ('125835','125833') --RCT.customer_trx_id
--AND trunc(aps.gl_date )&lt;=to_Date('05/31/2013','MM/DD/YYYY')
--AND trunc(aps.gl_date_closed) &gt;  to_Date('05/31/2013','MM/DD/YYYY')
--AND trunc(aps.gl_date )&lt;= trunc(TO_DATE(:P_THROUGH_DATE,'RRRR/MM/DD HH24:MI:SS'))
--AND trunc(aps.gl_date_closed) &gt;  trunc(TO_DATE(:P_THROUGH_DATE,'RRRR/MM/DD HH24:MI:SS'))
--and upper(RCT.trx_number) like '%RET%'
 --and  aps.status &lt;&gt; 'CL' */</property>
