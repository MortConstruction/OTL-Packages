                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT aaa.customer_trx_id ,aaa.gl_date, 'Adjustment' PaymentType,  SUM(aaa.amount) as l_adj_amt
            FROM   apps.ar_adjustments_all aaa
            WHERE  1=1
            /*AND    aaa.customer_trx_id = l_trx_id
             AND    aaa.gl_date &lt;= l_as_of_date */
            GROUP BY aaa.customer_trx_id,aaa.gl_date
Union all 
/*Partial Payments*/
 SELECT ara.applied_customer_trx_id
         ,ara.gl_date
         , 'PartialPayment' PaymentType,
        SUM(case ara.display when 'Y' then ara.amount_applied else 0 end) as p_partial_amt
       /* SUM(case ara.display when 'Y' then 0 else ara.amount_applied end) as p_historical_amt*/
        FROM   apps.ar_receivable_applications_all ara
        WHERE  1=1 
        and ara.applied_customer_trx_id is not null /*2 adjustment which net to .01 are eliminated here*/
       /* AND    ara.applied_customer_trx_id = p_trx_id
          AND    ara.gl_date &lt;= to_Date('03/31/2013','MM/DD/YYYY') --p_as_of_date  */
          AND    ara.status = 'APP'
          AND    ara.display='Y'
        GROUP BY ara.applied_customer_trx_id ,ara.gl_date
Union all 
/*Historical Payments*/
SELECT ara.applied_customer_trx_id
         ,ara.gl_date
         , 'HistoricalPayment' PaymentType,
       /* SUM(case ara.display when 'Y' then ara.amount_applied else 0 end) as p_partial_amt*/
        SUM(case ara.display when 'Y' then 0 else ara.amount_applied end) as p_historical_amt
        FROM   apps.ar_receivable_applications_all ara
        WHERE  1=1 
        and ara.applied_customer_trx_id is not null /*2 adjustment which net to .01 are eliminated here*/
       /* AND    ara.applied_customer_trx_id = p_trx_id
          AND    ara.gl_date &lt;= to_Date('03/31/2013','MM/DD/YYYY') --p_as_of_date  
         AND    ara.status &lt;&gt; 'APP'*/
          AND    ara.display&lt;&gt;'Y'
        GROUP BY ara.applied_customer_trx_id ,ara.gl_date
        having  SUM(case ara.display when 'Y' then 0 else ara.amount_applied end) &lt;&gt; 0</property>
