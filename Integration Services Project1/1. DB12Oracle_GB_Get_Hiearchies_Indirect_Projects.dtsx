              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select pa.Carrying_Out_Organization_id,pa.Segment1,pa.Name,pa.start_date,
 pa.completion_date,pa.org_id ,
(case pa.project_type  when 'Department' then 'Indirect' else 'Direct' end)  ProjectType,
pa.project_id,
pa.project_status_code,
cl.MajorBuildingType,
cl.MinorBuildingType,
cl.ContractType,
cl.BidType
from apps.pa_projects_all pa
LEFT JOIN (
SELECT   pa.Project_id,
               MAX(CASE class_category
                WHEN 'Major Building Type'
                   THEN class_code
                ELSE ''
             END) AS MajorBuildingType,
         MAX(CASE class_category
                WHEN 'Minor Building Type'
                   THEN class_code
                ELSE ''
             END) AS MinorBuildingType,
         MAX(CASE class_category
                WHEN 'Contract Type'
                   THEN class_code
                ELSE ''
             END) AS ContractType,
         MAX(CASE class_category
                WHEN 'Bid Type'
                   THEN class_code
                ELSE ''
             END) AS BidType
FROM     apps.pa_projects_all pa  JOIN apps.PA_PROJECT_CLASSES_V pv  ON pa.project_id = pv.project_id
WHERE    1 = 1
AND      (   Class_Category IN('Major Building Type', 'Minor Building Type', 'Bid Type', 'Contract Type', Null)
          OR Class_Category IS NULL)
GROUP BY 
         pa.project_id
) CL 
on PA.project_id = CL.project_id
where 
pa.project_type &lt;&gt; 'xxDepartment'
and
 pa.template_flag &lt;&gt; 'Y' 
order by 1</property>
