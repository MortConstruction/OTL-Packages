                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select flex_value_meaning,description,1 as GL_Segment_Number,'Company' as GL_Segment_Description from apps.FND_FLEX_VALUES_tl where FLEX_VALUE_ID in
  (SELECT Flex_value_id FROM apps.FND_FLEX_VALUES WHERE FLEX_VALUE_SET_ID IN 
  (SELECT FLEX_VALUE_SET_ID FROM apps.FND_FLEX_VALUE_SETS WHERE FLEX_VALUE_SET_NAME LIKE 'MORT_GL_COMPANY%'))
 union
  select flex_value_meaning,description,2 as GL_Segment_Number,'Group' as GL_Segment_Description 
  from apps.FND_FLEX_VALUES_tl where FLEX_VALUE_ID in
  (SELECT Flex_value_id FROM apps.FND_FLEX_VALUES WHERE FLEX_VALUE_SET_ID IN 
  (SELECT FLEX_VALUE_SET_ID FROM apps.FND_FLEX_VALUE_SETS WHERE FLEX_VALUE_SET_NAME LIKE 'MORT_GL_GROUP%'and summary_flag = 'N'and Compiled_value_attributes like 'Y%'))
 union
  select flex_value_meaning,description ,3 as GL_Segment_Number,'Function' as GL_Segment_Description  
  from apps.FND_FLEX_VALUES_tl where FLEX_VALUE_ID in
  (SELECT Flex_value_id FROM apps.FND_FLEX_VALUES WHERE FLEX_VALUE_SET_ID IN 
  (SELECT FLEX_VALUE_SET_ID FROM apps.FND_FLEX_VALUE_SETS WHERE FLEX_VALUE_SET_NAME LIKE 'MORT_GL_FUNCTION%'and summary_flag = 'N'and Compiled_value_attributes like 'Y%'))
  union
  select flex_value_meaning,description ,4 as GL_Segment_Number,'Account' as GL_Segment_Description
  from apps.FND_FLEX_VALUES_tl t where FLEX_VALUE_ID in
  (SELECT Flex_value_id FROM apps.FND_FLEX_VALUES WHERE FLEX_VALUE_SET_ID IN 
  (SELECT FLEX_VALUE_SET_ID FROM apps.FND_FLEX_VALUE_SETS WHERE FLEX_VALUE_SET_NAME LIKE 'MORT_GL_ACCOUNT%'and summary_flag = 'N'and Compiled_value_attributes like 'Y%'))
  order by 3,1</property>
